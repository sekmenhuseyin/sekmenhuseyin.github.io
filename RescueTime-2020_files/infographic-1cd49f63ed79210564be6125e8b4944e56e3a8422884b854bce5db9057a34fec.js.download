    var setup_daily_timeline = function(element, data_point) {
        element = $(element).get(0);

		var params = {};

		params.element = $(element).get(0);

    	params.data_point = data_point;

        params.data = group_by_month(RTDATA.daily_intervals, params.data_point);



        params.max_duration = d3.max(RTDATA.daily_intervals, function(d) { return d[data_point]; } );

        // I'm SURE There's a cleaner way to do this and get max_duration and max_value at the same time.
        params.max_date_object = RTDATA.daily_intervals.find(function(d) { return d[data_point] == params.max_duration });

        params.max_date = date_parser(params.max_date_object['summary_date']);

        params.chart_div = d3.select(params.element);

        params.tooltip = params.chart_div.select('div#all-time-durations-tooltip');

        params.padding = {
            left: 3,
            top: 3,
            right: 3,
            bottom: 3
        };

        var position_barchart_tooltip = function(d) {
            var mouse = d3.mouse(element),
            tooltip_left = mouse[0] + 10,
            tooltip_top = mouse[1] - 20;

            params.tooltip.style('top', tooltip_top+'px');
            params.tooltip.style('left', tooltip_left+'px');
        };

        var update_barchart_tooltip = function(key) {
            var date = key['date'];
            var tooltip_format = d3.time.format("%a, %b %e");
            params.tooltip.style('display','block');

            params.tooltip.select('div.tooltip-date').text(tooltip_format(date));
            params.tooltip.select('div.tooltip-description').text(duration_to_time( key['duration'] ) );
        };

        var hide_barchart_tooltip = function() {
            params.tooltip.style('display','none');
        };

        // fill out the contents of each month
        var draw_month  = function(d,i) {
            var month = d['date'],
                month_start_date = d['date'],
                month_end_date = d3.time.month.ceil(d3.time.second.offset(d['date'], 1));

            var container_width = $(this).width(),
                bar_area_width = container_width - (params.padding.left + params.padding.right),
                container_height = 54,
                bar_area_height = container_height - (params.padding.top + params.padding.bottom),
                day_width = Math.floor(container_width / d.days.length) - 2;


            // get or create the month label
            var month_label = d3.select(this).select('div.month-name');

            if (month_label.empty()) {
                var month_label = d3.select(this).append('div').attr('class', 'month-name').text(d['label']);
            }

            // get or create the svg we'll use for the charg
            var month_chart = d3.select(this).select('svg.month-chart');
            if (month_chart.empty()) {
                var month_chart = d3.select(this).append('svg').attr('class', 'month-chart');
                var max_day_group = month_chart.append('g').attr("transform", "translate(0,0)").attr('class', 'max-day-group');
                var month_day_group = month_chart.append('g').attr("transform", "translate("+ params.padding.left +","+ params.padding.top +")").attr('class', 'month-day-group');

            } else {
                var max_day_group = d3.select(this).select('g.max-day-group');
                var month_day_group = d3.select(this).select('g.month-day-group');
            }

            var day_scale = d3.time.scale().domain([month_start_date, month_end_date]).range([0, bar_area_width])
                duration_scale = d3.scale.linear().domain([0,params.max_duration]).range([0,bar_area_height]);



            // if this month contains the max_date, highlight it.
            var max_day = [];

            if (params.max_date.getMonth() == month.getMonth()) {
                max_day = [params.max_date_object];
            }

            var max_day_marker = max_day_group.selectAll('.max-day').data(max_day, function(md) { return md['summary_date']; } );

            max_day_marker.enter().append('svg:rect');
            max_day_marker.exit()
                .transition()
                .duration(1000)
                .attr('height', 0)
                .attr('y', container_height)
                .remove();

            max_day_marker
                .attr('class', 'max-day')
                .attr('fill', '#FFDD29')
                .attr('height', 0)
                .attr('y', container_height)
                .transition()
                .duration(1000)
                .attr('x', function(dd) { return day_scale(date_parser(dd['summary_date'])) - (day_width/2); })
                .attr('width', day_width*3)
                .attr('y', 0)
                .attr('height', container_height)
                .attr('fill', '#FFDD29');




            // draw the bars

            var month_days = month_day_group.selectAll('.month-day').data(d.days, function(dd) { return dd['date'];});

            month_days.enter().append('svg:rect').attr('fill', function(dd) { return dd['color']; });
            month_days.exit()
                .transition()
                .duration(1000)
                .attr('height', 0)
                .attr('y', function(dd) { return bar_area_height; })
                .remove();

            month_days
                .attr('class', 'month-day')
                .on("mouseover",function(d){
                    update_barchart_tooltip(d);
                })
                .on("mousemove", function(d) {
                    position_barchart_tooltip(d);
                })
                .on("mouseout", function(d) {
                    hide_barchart_tooltip(d);
                })
                .on("click", function(dd) {
                    show_activities_for_date(dd['date']);
                })
                .transition()
                .duration(1000)
                .attr('x', function(dd) { return day_scale(dd['date']); })
                .attr('width', day_width)
                .attr('y', function(dd) { return bar_area_height - duration_scale(dd['duration']); })
                .attr('height', function(dd) { return duration_scale(dd['duration']); })
                .attr('fill', function(dd) { return dd['color']; });




        }

        // we'll need to fill out a grid of months with divs so it'll be responsive. EEEEEP!
        params.month_elements = params.chart_div.selectAll('div.month-container').data(params.data, function(d, i) { return i; });

        params.month_elements.enter().append('div');
        params.month_elements.exit().remove();

        params.month_elements
            .attr('class', 'month-container large-3 small-12 columns')
            .each(draw_month);


    }


