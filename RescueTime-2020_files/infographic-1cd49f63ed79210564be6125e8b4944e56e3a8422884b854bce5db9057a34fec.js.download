// construct a url based on info from the report_context object (with fallback to RTDATA.report_context)
// Kludge alert: if you need to rely on the current report context but aren't modifying it, pass in an empty object as the argument
// If you want the current report_context info completely excluded from the url, don't pass anything in or pass in a null value
RT.url = function(path_info, include_domain) {
  var path = [],
      query_string = {},
      report_context = {};

  if (include_domain) {
    path.push(document.location.protocol+'//'+document.location.host);
  } else {
    path.push(''); // push an empty element on the start of the array so we get the leading slash
  }

  if (typeof(path_info) === 'string') {
    path.push(path_info);
    report_context = null;
  } else if (typeof(path_info) === 'object') {
    report_context = $.extend({}, RTDATA.report_context, path_info); // merge in the path_info data on top of the existing page report context
  }

  if (report_context !== null) {

    var page = report_context.page || RTDATA.report_context.page,
        time_frame,
        by;

    path.push(page);

    if (page === 'browse') {
      var taxonomy = report_context.taxonomy || RTDATA.report_context.taxonomy || null;
      if (taxonomy !== null) {
        path.push(taxonomy.name);

        var taxon = report_context.taxon || RTDATA.report_context.taxon || null;
        if (taxon !== null) {
          path.push(taxon.id);
        }



        if (report_context.action !== undefined && report_context.action !== 'index') {
          // handle the specialty browse pages
          path.push(report_context.action);
        } else {
          by = report_context.by || RTDATA.report_context.by;
          time_frame = report_context.time_frame || RTDATA.report_context.time_frame;

          // handle all goals report by day
          if(taxonomy.name.toLowerCase() === 'goals' && time_frame === 'day' && taxon === null){
            by = 'day';
          }

          path.push('by/' + by);
        }
      }
    }

    time_frame = report_context.time_frame || RTDATA.report_context.time_frame;
    if (time_frame !== null) {
      path.push('for/the/'+time_frame+'/of');
    }

    var date = report_context.date || iso(RTDATA.report_context.date);
    if (date !== null) {
      if (typeof(date) === 'string') {
        date = iso(date);
      }
      path.push(date.getFullYear()+'-'+(date.getMonth()+1)+'-'+date.getDate());
    }

    // handle general query string params, be careful with these
    if (typeof(report_context.query_string_params) === 'object') {
      query_string = report_context.query_string_params;
    }

    // if there are keywords or search id, tack them onto the query string.
    if (page === 'browse') {
      if (report_context.keywords != null) {
        query_string.keywords = report_context.keywords;
      } else if (report_context.search_id != null) {
        query_string.search_id = report_context.search_id;
      }
    }

    if (report_context.schedule_id != null && report_context.schedule_id >= 0) {
      query_string.schedule_id = report_context.schedule_id;
    }

    // only append the query string if it exists
    if ($.param(query_string).length > 0) {
      path.push('?'+$.param(query_string));
    }

  }

  return path.join('/').toLowerCase();
};

RT.go = function(report_context) {

  var path = RT.url(report_context);

  if (typeof(Turbolinks) === 'object') {
    console.log('TURBOLINKS: visiting ' + path);
    Turbolinks.visit(path);
  } else {
    window.location = path;
  }
};

