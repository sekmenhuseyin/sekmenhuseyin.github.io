// need a way to execute code when the dom is ready in either a traditional page load AND a turbolinks query
function on_dom_ready(f) {
  if (typeof(Turbolinks) === 'object') {
    // use jquery.one to attach the event so it doesn't get executed on subsequent page loads
    $(document).on('turbolinks:load', f);
  } else {
    // JQuery(document).ready will get exectued immediately when in turbolinks environment, which may or may not be what you want.
    $(document).ready(f);
  }
}

