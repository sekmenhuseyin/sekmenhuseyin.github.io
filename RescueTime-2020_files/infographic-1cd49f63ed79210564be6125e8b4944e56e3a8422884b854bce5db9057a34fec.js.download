  var browse_usage_breakdown_bar_chart = function(element, opts){

    var chart = d3.select(element),
        item_id = chart[0][0].dataset.id,
        local_max = chart[0][0].dataset.max,
        data = RTDATA.chart_data[opts.type][item_id],
        margin = {top: 3, right: 0, bottom: 0, left: 6},
        width = $(element).parent().width() - margin.left - margin.right,
        height = $(element).parent().height() - margin.top,
        stroke_dasharray = '3, 1',
        bar_width = 4,
        circle_min_y_pos = 53.5,
        circle_radius = 3,
        tooltip_y_offset = 36,
        transition_speed = 1000,
        colors = {
          slate:  "#667B8B",
          green:  "#65A870",
          green_dark: "#41874d",
          white:  "#FFFFFF",
          grey:   "#DDDDDD"
        };

    function re_draw(){
      svg.selectAll('rect')
        .transition(transition_speed)
          .attr('y', function(d) { return y(d.duration); })
          .attr('height', function(d) { return height - y(d.duration); });

      svg.selectAll('circle')
        .transition(transition_speed)
          .attr('cx', function(d) {
            return x(iso(d.interval)) + (bar_width/2);
          })
          .attr('cy', function(d) { return d3.min([(y(d.duration) + circle_radius),circle_min_y_pos]); });
    }

    // D3
    var start_time = iso(opts.start_date),
        end_time = d3.time.second.offset(iso(opts.end_date), 1),
        report_interval = RTDATA.report_context.interval,
        time_frame = RTDATA.report_context.time_frame,
        intervals = d3.time.days(start_time, end_time);

    if (time_frame === 'day') {
      intervals = d3.time.hours(start_time, end_time, 3);
    } else if (time_frame === 'year') {
      intervals = d3.time.weeks(start_time, end_time,4);
    }

    var x = d3.time.scale().range([0, width]).domain([start_time, end_time]),
        y = d3.scale.linear().range([height, 0]).domain([0, opts.global_max]);

    var svg = chart.append('svg')
                .attr('width', width + margin.left + margin.right)
                .attr('height', height + margin.top + margin.bottom)
                .append('g')
                  .attr('transform',
                        'translate(' + margin.left + ',' + margin.top + ')');

    //// Draw the headings
    var head_wrap = $('DIV#team-activities-headings-wrapper');
    ////// Check if we've already drawn them
    if(head_wrap.find('.heading-label-wrapper').length === 0){
      if(time_frame === 'week' || time_frame === 'day'){
        head_wrap = d3.select(head_wrap[0]);
        var head_svg = head_wrap.append('svg')
                        .attr('width', width + margin.left + margin.right)
                        .attr('height', height + margin.top + margin.bottom)
                        .attr('class', 'heading-label-wrapper')
                        .append('svg:g')
                          .attr('transform', 'translate(' + margin.left + ',' + margin.top + ')');
        var text = head_svg.selectAll('g')
                    .data(intervals)
                    .enter()
                      .append('text')
                        .attr('x', function(d) { var interval_adjustment = time_frame === 'day' ? 9 : 0; return x(d) - (bar_width/2) - interval_adjustment; })
                        .attr('y', (height-9))
                        .text( function (d) { return interval_short_label(iso(d), report_interval, time_frame); });
      }
    }

    //// Draw the grid lines
    var x_grid_wrapper = svg.append('svg:g').attr('class', 'x-grid-wrapper');
    x_grid_wrapper.selectAll('g')
      .data(intervals)
      .enter().append('line').attr('class','xline')
        .attr('y1', 0)
        .attr('x1', function(d) { return x(d) + (bar_width / 2); })
        .attr('x2', function(d) { return x(d) + (bar_width / 2); })
        .attr('y2', height)
        .attr('stroke-width', 1)
        .attr('stroke', colors.grey)
        .style('stroke-dasharray', (stroke_dasharray));

    //// Draw the bars
    svg.selectAll('bar')
      .data(data)
      .enter().append('rect')
        .style('fill', colors.green)
        .attr('data-id',function(d){ return iso(d.interval).getTime(); })
        .attr('x', function(d) { return x(iso(d.interval)); })
        .attr('y', function(d) { return y(d.duration); })
        .attr('width', bar_width + 'px')
        .attr('height', function(d) { return height - y(d.duration); });

    //// Define the point group and the tooltip container
    var point = svg.append('svg:g')
                  .data(data)
                  .attr('class', 'line-point'),
        tooltipDiv = chart.append('div')
                      .data(data)
                      .attr('class', 'tooltip')
                      .style('display', 'none');

    //// Draw the circles in the point group
    point.selectAll('circle')
      .data(data)
      .enter()
        .append('circle')
          .attr('cx', function(d) { return x(iso(d.interval)) + (bar_width / 2); })
          .attr('cy', function(d) { return d3.min([(y(d.duration) + circle_radius), circle_min_y_pos]); })
          .attr('r', function(d) { return d.duration === 0 ? 0 : circle_radius; })
          .style('fill', colors.white)
          .attr('stroke-width', '1')
          .style('stroke', colors.slate)
          .style('opacity', 1);

    //// Create the circle hover events
    point.selectAll('circle')
      .on('mouseover', function(d) {
        y.domain([0, local_max]);
        var activeCircle = d3.select(this);
        activeCircle
          .style('fill', colors.slate)
          .style('cursor', 'pointer');
        tooltipDiv
          .html(interval_full_label(iso(d.interval), report_interval, time_frame) + ': ' + d.duration_formatted)
          .style('display','block')
          .style('top', (d3.min([(y(d.duration) + circle_radius), circle_min_y_pos]) - tooltip_y_offset) + 'px')
          .style('left', function(){
            var tipWidth = $(this).width(),
                circlePos = Math.round(parseInt(activeCircle.attr('cx')));
            return ((circlePos + margin.left) - (tipWidth/2)) + bar_width + 'px';
          });
          svg.selectAll('rect[data-id="' + iso(d.interval).getTime() + '"]')
          .style('fill', colors.green_dark);
      })
      .on('mouseout', function(d) {
        d3.select(this)
          .style('fill', colors.white);
        tooltipDiv
          .style('display','none');
        svg.selectAll('rect[data-id="' + iso(d.interval).getTime() + '"]')
          .style('fill', colors.green);
      });

    // Rescale graphs on container hover
    $(element).parents('DIV.activity-detail-item').on('mouseover', function(d){
      y.domain([0, local_max]);
      re_draw();
    }).on('mouseout', function(d){
      y.domain([0, opts.global_max]);
      re_draw();
    });
  };

  var search_panel_timeout;
