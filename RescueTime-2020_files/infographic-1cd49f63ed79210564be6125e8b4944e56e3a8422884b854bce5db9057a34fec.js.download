RT.devices = (function () {
  // private stuff
  var types = [],
      active_type = null;

  var init = function() {
    console.log('DEVICES INITIALIZED');

    // empty out the types array and repopulate it.
    // this allows us to re-init the page after a turbolinks refresh
    types = [];
    active_type = null;

    var device_types_for_init = [
      {name:'mobile', test: "only screen and (max-width: 48em)"},
      {name:'desktop', test: "only screen and (min-width: 48em)"}
    ];

    types = device_types_for_init;

    // roll through and add some extra flags to eacy 'type' declaration
    for(var i = 0; i < device_types_for_init.length; i+=1) {
      // it hasn't been initialized yet
      types[i].initialized = false;
    }

    active_type = current();
    draw_current();

    // set the active view
    // $(document).on('turbolinks:load', function() {
    //   active_type = current();
    //   console.log('CALLING DRAW CURRENT FROM INIT');
    //   draw_current();
    // });

    // set up a few listeners to make sure we keep the display updated.
    $(window).bind('orientationchange resize', function(){
      var current_type = current();
      if (active_type === null || active_type.name !== current_type.name) {
        active_type = current_type;
        draw_current();
      }
    });
  };

  // draw the current view
  // note: in a perfect world, this wouldn't be necessary. This is only meant for stuff that is drastically different between views
  // put everything that can get drawn universally into document.ready
  // to keep it simple, try to:
  //  - include teardown code in the draw code itself, so we don't have to maintain a seperate teardown event
  //  - have the draw code manage loading any extra data / scripts from the server (in other words, be smart enough to elminiate redundant requests)
  var draw_current = function(){
    // call the draw event for the current type
    // you can register events with something like:
    // $(document).on('mobileDraw', function(){ foo... });
    // If what you are doing is universal across view types, use $(document).ready(function(){ foo... }) instead.
    if(active_type != null){
      console.log('calling draw events for ' + active_type.name);
      $.event.trigger(active_type.name+'Draw');

      if(active_type.name === 'mobile'){
        $('BODY').addClass('mobile').removeClass('desktop');
      } else {
        $('BODY').addClass('desktop').removeClass('mobile');
      }
    }
  };

  // public functions

  // return the current device type
  var current = function(){
    for(var i=0;i<types.length;i+=1){
      if(Modernizr.mq(types[i].test)){
        return types[i];
      }
    }
  };

  var is = function(type){ // test for a specific device type
    for(var i=0;i<types.length;i+=1) {
      if (types[i].name === type) {
        return Modernizr.mq(types[i].test);
      }
    }
  };

  var reset = function(){
    init();
  };

  //// once I removed this, turbolinks stopped calling draw functions multiple times - @brianarenz
  // init();

  // return publicly accessible functions
  return {
    is: is,
    current: current,
    reset: reset
  };
