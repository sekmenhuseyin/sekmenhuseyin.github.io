            return interval_full_label(d, RTDATA.report_context.interval, RTDATA.report_context.time_frame);
          }
        })
        .attr('x', function(d) { return params.scales.x0(d) + (params.group_width / 2); })
        .attr('y', params.bar_area_height + 20)
        .attr('text-anchor', 'middle');

      // interval label ticks (not filtered like the actual labels are)
      params.label_wrapper.selectAll('.xlabeltick')
        .data(params.intervals)
        .enter().append('svg:line')
        .attr('class', 'xlabeltick')
        .attr('x1', function(d) { return params.scales.x0(d) + (params.group_width / 2); })
        .attr('y1', params.bar_area_height)
        .attr('x2', function(d) { return params.scales.x0(d) + (params.group_width / 2); })
        .attr('y2', params.bar_area_height + 5)
        .attr('text-anchor', 'middle');


    } else {
      params.label_wrapper.selectAll('.xlabel')
        .data(params.data[0])
        .enter().append('svg:text')
        .attr('class', 'xlabel')
        .text(function(d) {
          return d.short_name.substring(0,20);
        })
        .attr('x', function(d, i) { return params.scales.x0(i) + (params.group_width / 2) + 10; })
        .attr('y', params.bar_area_height + 10)
        .attr('text-anchor', 'end')
        .attr('transform', function(d, i) {
          return 'rotate(315, ' + Math.round(params.scales.x0(i) + (params.group_width / 2) + 10) + ', ' + Math.round(params.bar_area_height + 18) + ')';
        });
    }
  };

