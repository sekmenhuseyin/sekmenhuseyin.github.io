on_dom_ready(function(){
  var $element = $('DIV#async-year-report');
  if($element.length === 0) return;

  window.clearInterval(window.async_year_report_interval);
  // set spinner state
  $('DIV#page-content-wrapper').before('<div id="turbolinks-loading"><span></span></div>');
  $('BODY').addClass('fetching-page');

  var year = iso(RTDATA.report_context.date).getFullYear();
  var x = 0;
  window.async_year_report_interval = setInterval(function () {
    pollYearReport(year);
    if (++x === 20) {
      window.clearInterval(window.async_year_report_interval);
    }
  }, 5000);
});

function pollYearReport(year){
  $.ajax({
    url: '/poll-year-report/'+ year,
    type: 'GET',
    success: function(res) {

      if(res.reload){
        // found the cache key
        // clear  polling interval
        window.clearInterval(window.async_year_report_interval);
        // unset spinner state
        $('DIV#turbolinks-loading').remove();
        $('BODY').removeClass('fetching-page');
        // turbolinks reload page via original report_context params
        RT.go(RTDATA.report_context);
      }
    },
    error: function(err){
      console.log('ERROR', err);
    }
  });
}

