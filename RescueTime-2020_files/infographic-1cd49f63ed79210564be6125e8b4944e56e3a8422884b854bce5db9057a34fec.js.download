    // need to get a start and end time to build the graph correctly.
    var start_time = RT.time_frame_start(),
        end_time = RT.time_frame_end(),
        intervals = RT.time_frame_intervals(),
        interval_width = width_with_padding / intervals.length,
        interval_offset = interval_width/2;

    // make sure the name is something that can be safely formatted into a variable name
    var sanitize_name = function(str) {
      if (typeof(str) === 'string') {
