          }
          // if we haven't returned yet, pick a random color
          return chart_item_color(dd,di);
        })
        .transition()
        .duration(1000)
        .attr('y', function(dd) {
          return -y(dd.duration);
        })
        .attr('height', function(dd) {
          return y(dd.duration);
        });
    }

    // draw the baseline
    chart.append('line')
      .attr('class', 'spotlight-baseline')
      .attr('x1', 0)
      .attr('y1', 0)
      .attr('y2', 0)
      .attr('x2', width_with_padding)
      .style('stroke-width', 2)
      .style('stroke', '#444');
  };


  /* DAILY PATTERNS CHART - BUBBLE MAP */
