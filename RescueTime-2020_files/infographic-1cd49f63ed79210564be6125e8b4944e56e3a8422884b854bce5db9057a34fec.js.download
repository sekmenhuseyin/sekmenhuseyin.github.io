    // popuplate the content of the tooltip
    var populate_interval_tooltip = function(name, total_duration, detail_label, detail_duration) {
      var tooltip_name = name;
      tooltip_total_duration = duration_to_time(total_duration) + " Total";

      var formatted_label,
          str = detail_label.split('_');

      if(str[0] === 'ios'){
        formatted_label = 'iOS Device';
      } else {
        for(var i=0;i<str.length;i++){
          str[i] = str[i][0].toUpperCase() + str[i].substr(1);
        }
        formatted_label = str.join(' ');
      }

      tooltip_detail_duration = duration_to_time(detail_duration) + " " + formatted_label;

      tooltip_text_has_been_updated = true;


    };

    function hide_interval_tooltip() {
      // tooltip.select('.spotlight-tooltip-name')
      //   .text('');
      // tooltip.select('.spotlight-tooltip-duration')
      //   .text('');
      tooltip.style('display','none');
    }

    function draw_y_axis_lines() {

      // figure out a y axis tick mark interval that won't be overwhelming visually.
      var tick_interval = (600*60); // every ten hours

      if (max_duration < 7200) { // probably talking about hour intervals
        tick_interval = (15*60); // 15 minutes
      } else if (max_duration < 18000) { // about half a day
        tick_interval = (60*60); // every hour
      } else if (max_duration < 43200) { // less than 12 hours 
        tick_interval = (120*60); // every two hours
      } else if (max_duration < 86400) { // less than 24 hours
        tick_interval = (240*60); // every four hours
      }

      var y_label_wrapper = chart.append('svg:g').attr('class', 'y-label-wrapper');

      var current_y = 0;
      while((current_y+tick_interval) < max_productive_duration) {
        current_y += tick_interval;
        y_label_wrapper.append('line').attr('class','ybaseline')
          .attr('x1', 0)
          .attr('y1', y(-(max_distracting_duration+current_y)))
          .attr('y2', y(-(max_distracting_duration+current_y)))
          .attr('x2', bar_area_width + padding[1])
          .attr('stroke-width', 1)
          .attr('stroke', '#EAEAEA');

        y_label_wrapper.append('svg:text')
          .attr('class', 'ylabel')
          .text(duration_to_time(current_y).split(' ')[0])
          .attr('x', -padding[3])
          .attr('y', y(-(max_distracting_duration+current_y))+3)
          .attr('text-anchor', 'left');
      }

      current_y = 0;
      while((current_y+tick_interval) < max_distracting_duration) {
        current_y += tick_interval;
        y_label_wrapper.append('line').attr('class','ybaseline')
          .attr('x1', 0)
          .attr('y1', y(-(max_distracting_duration-current_y)))
          .attr('y2', y(-(max_distracting_duration-current_y)))
          .attr('x2', bar_area_width + padding[1])
          .attr('stroke-width', 1)
          .attr('stroke', '#EAEAEA');

        y_label_wrapper.append('svg:text')
          .attr('class', 'ylabel')
          .text(duration_to_time(current_y).split(' ')[0])
          .attr('x', -padding[3])
          .attr('y', y(-(max_distracting_duration-current_y))+3)
          .attr('text-anchor', 'left');
      }

    }

    // this is defined in a more generic way above, but this chart doesn't set up the same defaults
    // so we need to implement differently.
    function draw_schedule_intervals() {
      schedule_wrapper.selectAll('.schedule_intervals')
      .data(intervals)
      .enter().append('svg:rect')
      .attr('class', function(d) {
        var classes = ['schedule_intervals'];
        var interval_type = 'hour';

        if (RTDATA.report_context.interval !== 'hour') {
          interval_type = 'day';
        }

        if (RT.is_time_within_working_hours(d, interval_type)) {
          classes.push('on-hours');
        } else {
          classes.push('off-hours');
        }
        return classes.join(' ');
      })
      .attr('x', function(d) { return x(d) - (bar_padding/2); })
      .attr('width', bar_width + bar_padding)
      .attr('y', -bar_area_height)
      .attr('height', bar_area_height);
    }
  };


