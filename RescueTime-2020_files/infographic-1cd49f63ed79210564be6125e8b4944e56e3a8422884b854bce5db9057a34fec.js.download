          var td = jQuery(link).parents('TD');
          jQuery(td).append('<table class="activity-detail-list"></table>');
          var details_table = jQuery(td).find('TABLE.activity-detail-list');
          var row_template = "<tr><td class='duration'>__DURATION__</td><td>__NAME__</td></tr>";
          for(var i=0;i<data.length;i++) {
            var duration = duration_to_time(data[i].sum_durations);
            var activity_name = data[i].name;
            jQuery(details_table).append(row_template.replace('__DURATION__', duration).replace('__NAME__', activity_name));
          }
          jQuery(link).text(placeholder_text);
          jQuery(link).addClass('report-loaded');
        },
        'json'
      );
    }

  });


  // GENERAL TIME SERIES REPORT SWITCHER
  // expects a set of elements that will have the following features:
  // 1. container element with the 'data-series-chart-id' attribute that has the ID of the chart it should control as it's value
  // 2. a series of elements with the 'data-series-id' attribute that has the ID of a dataset (stored in RTDATA.chart_data)
  // this function will manage the 'selected' state of the tabs and kick off a refresh on the time series chart.

  jQuery(document).on('click', '*[data-series-chart-id] *[data-series-id]', function(e) {
    // don't do anything if the clicked element is already selected
    if (jQuery(this).hasClass('selected') === false) {
      var series_name = jQuery(this).data('series-id');
      var chart_element_id = jQuery(this).parents('*[data-series-chart-id]').data('series-chart-id');

      if (chart_element_id != null) {
        var chart_element = jQuery('#' + chart_element_id).get(0);
        if (series_name != null) {
          jQuery(this).parents('*[data-series-chart-id]').find('*[data-series-id]').removeClass('selected');
          jQuery(this).addClass('selected');

          jQuery(chart_element).data('selected-series', series_name);
          RT.charts.compare_time_series(chart_element);

          // fire an event that can cause data elsewhere to respond to an update of this element
          jQuery(chart_element).trigger('time_series:updated');
        }
      }
    }
  });

  // SOURCE REPORTS SWITCHER
  jQuery(document).on('time_series:updated', 'DIV#sources_comparison_chart', function(e) {
    // var source_name = jQuery(this).data('series-id');
    var source_name = jQuery(this).data('selected-series');
    if (source_name != null) {
      // set the selected_source_data pointer to the correct data set
