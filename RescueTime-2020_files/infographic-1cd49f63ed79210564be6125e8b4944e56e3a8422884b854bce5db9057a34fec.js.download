    // scales
    var x = d3.scale.linear().domain([start_hour, end_hour+1]).range([0, data_area.width]),
        r = d3.scale.linear().domain([0,maximum_duration]).range([1, maximum_bubble_radius]),
        taxon_row_index = d3.scale.ordinal().domain(data.config.taxons.map(function(t) { return t.id; })).range(d3.range(0,data.config.taxons.length)),
        bar_colors = d3.scale.category20().domain([0, data.config.taxons.length-1]);

    chart_div.height(chart_area.height);

    // draw the chart svg
    var chart_svg = d3.select(element).append("svg:svg")
        .attr("id", 'bubble_map_chart')
        .attr("width", chart_area.width)
        .attr("height", chart_area.height);

    var setup_taxon_rows = function() {

      var chart_taxon_rows = chart_svg.append("svg:g")
        .attr('class', 'taxon-label-panel')
        .attr("transform", "translate(" + (padding.left) +", "+ (padding.top) + ")");

      // function to draw taxon labels and manage mouse events
      function draw_taxon_row(selection) {
        selection.append('rect')
          .attr('x', 0)
          .attr('y', 0)
          .attr('rx', 3)
          .attr('ry', 3)
          .attr('width', chart_area.width)
          .attr('height', row_height)
          .attr('class', 'background-row');

        selection.append('svg:text')
          .attr('x', 0)
          .attr('y', function(d) {
            if (layout_type === 'mobile') {
              return 9;
            } else {
              return 27;
            }
          })
          .text(function(d) {
            if (d.name.length > 25) {
              return d.name.slice(0,24) + '...';
            } else {
              return d.name;
            }
          })
          .on('click', function(d) {
            RT.go(d.detail_url.replace(/^\//, ''));
          });

        // draw the bubbles within the group so we can get mouse events
        // on the row and the bubbles.
        var row_intervals = selection.selectAll('g.interval')
          .data(function(d) {
            return get_taxon_hours(d.id);
          });

        row_intervals.enter().append('svg:g');

        row_intervals
          .attr('class', 'interval')
          .attr("transform", function(d, i) {
            var row_interval_top = 0;
            return "translate("+ (label_width + x(d.hour)) +", "+ row_interval_top +")";
          })
          // .append('svg:circle')
          // .attr('class', function(d) { return 'duration-' + d.duration; })
          // .attr('cx', column_width/2)
          // .attr('cy', row_height/2)
          // .attr('r', 0)
          .append('svg:rect')
          .attr('class', function(d) { return 'duration-' + d.duration; })
          .attr('width', column_width-8)
          .attr('height', 0)
          .attr('x', 4)
          .attr('y', row_height/2)
          .attr('rx', 3)
          .attr('ry', function(d) {
            if (r(d.duration) > 3) {
              return 3;
            } else {
              return 1;
            }
          })
          .attr('fill', function(d, i) {
            var taxon_index = taxon_row_index(d.id);
            return chart_item_color(data.config.taxons[taxon_index], d.id, bar_colors);
          })
          .attr('opacity', 1)
          .on('mouseover', function(d) {
            show_tooltip(d, d3.mouse(element));
          })
          .on('mousemove', function(d) {
            position_tooltip(d3.mouse(element));
          })
          .on('mouseout', function(d) {
            hide_tooltip(d);
          })
          .transition()
            .duration(2000)
            // .attr('r', function(d) { return r(d.duration); });
            .attr('height', function(d) { return r(d.duration)*2; })
            .attr('y', function(d) { return (row_height/2) - r(d.duration); });

      }

      var get_taxon_hours = function(taxon_id) {
        return data.hours.map(function(hour) {
          var taxon_hour = hour.taxons.filter(function(t) { return t.id === taxon_id; });
          if (typeof(taxon_hour[0]) !== 'undefined') {
            return {hour: hour.hour, duration: taxon_hour[0].duration, id: taxon_hour[0].id};
          }
        }).filter(function(h) { return h != null && typeof(h) !== 'undefined'; });
      };

      chart_taxon_rows.selectAll('.item-label')
        .data(data.config.taxons)
        .enter().append('svg:g')
        .attr('class', function(d) {
          return 'chart-taxon-row taxon-row-'+d.id;
        })
        .attr('transform', function(d,i) {
          var taxon_row_height = (i*row_height);
          if (layout_type === 'mobile') {
            taxon_row_height = (i*(row_height+mobile_label_height));
          }
          return "translate(0, "+ taxon_row_height + ")";
        })
        .on('mouseover', function(d) {
          d3.select(this).classed('focused', true);
          d3.select(this).select('text').transition().attr('x', 9);
          focus_row(d.id);
        })
        .on('mouseout', function(d) {
          d3.select(this).classed('focused', false);
          d3.select(this).select('text').transition().attr('x', 0);
          unfocus_row(d.id);
        })
        .call(draw_taxon_row);

      return chart_taxon_rows;

    };

    var setup_interval_labels = function() {
      var chart_top_interval_labels = chart_svg.append("svg:g")
        .attr('class', 'label-panel')
        .attr("transform", "translate(0,0)");

      var chart_bottom_interval_labels = chart_svg.append("svg:g")
        .attr('class', 'label-panel')
        .attr("transform", "translate(0,"+ (chart_area.height-1) +")");

      var top_label_padding = 27;
      if (layout_type === 'mobile') {
        top_label_padding = 1;
      }

      // draw baselines on the top and bottom of the chart
      chart_top_interval_labels.append("svg:line")
        .attr('x1', 0)
        .attr('x2', chart_area.width)
        .attr('y1', top_label_padding)
        .attr('y2', top_label_padding)
        .attr('stroke', '#444')
        .attr('stroke-width', 1);

      chart_bottom_interval_labels.append("svg:line")
        .attr('x1', 0)
        .attr('x2', chart_area.width)
        .attr('y1', 0)
        .attr('y2', 0)
        .attr('stroke', '#444')
        .attr('stroke-width', 1);

      // draw tick marks
      var chart_top_interval_ticks = chart_top_interval_labels.selectAll('line.tick-line')
        .data(hours)
        .enter()
        .append("svg:line")
        .attr('class', 'tick-line')
        .attr('x1', function(d) {
          return label_width + x(d) + (column_width/2);
        })
        .attr('x2', function(d) {
          return label_width + x(d) + (column_width/2);
        })
        .attr('y1', top_label_padding)
        .attr('y2', top_label_padding+13);

      var chart_bottom_interval_ticks = chart_bottom_interval_labels.selectAll('line.tick-line')
        .data(hours)
        .enter()
        .append("svg:line")
        .attr('class', 'tick-line')
        .attr('x1', function(d) {
          return label_width + x(d) + (column_width/2);
        })
        .attr('x2', function(d) {
          return label_width + x(d) + (column_width/2);
        })
        .attr('y1', -13)
        .attr('y2', 0);


      // add the text labels to the top
      if (layout_type === 'mobile') {

      } else {
        var chart_top_text_labels = chart_top_interval_labels.selectAll('text.interval-label')
          .data(hours)
          .enter().append('svg:text')
          .attr('class', 'interval-label')
          .attr('y', 18)
          .attr('x', function(d) {
            return label_width + x(d) + (column_width/2);
          })
          .attr('text-anchor', 'middle')
          .text(function(d) {
          if (RTDATA.user_context.hour_format_preference === 24) {
            return d+ ":00";
          } else if (d === 0) {
            return 12;
          } else if (d === 12) {
            return 'noon';
          } else if (d > 12) {
            return (d-12) + 'pm';
          } else {
            return d + 'am';
          }
        });
      }
    };

    var setup_chart_tooltip = function() {
      // add a tooltip container
      var tooltip = d3.select(element).append("div")
        .attr('class','context-tooltip');
      tooltip.append('div')
        .attr('class', 'context-tooltip-name');
      tooltip.append('div')
        .attr('class', 'context-tooltip-text');
      tooltip.append('div')
        .attr('class', 'context-tooltip-duration');

      return tooltip;
    };

    var setup_empty_hours = function() {
      var chart_empty_hours = chart_svg.append("svg:g")
        .attr('class', 'taxon-empty-hours')
        .attr("transform", "translate(" + (padding.left + label_width) +", "+ (padding.top) + ")");

      var chart_empty_columns = chart_empty_hours.selectAll('line.empty-hour-column')
        .data(get_empty_hours());

      chart_empty_columns.enter().append('svg:line');

      chart_empty_columns
        .attr('class', 'empty-hour-column')
        .attr('x1', function(d) { return x(d) + (column_width/2); })
        .attr('x2', function(d) { return x(d) + (column_width/2); })
        .attr('y1', 0)
        .attr('y2', data_area.height+12);

      return chart_empty_hours;
    };

    var focus_row = function(row_id) {
      // get the max-duration for THIS row
      var maximum_row_duration = d3.max(data.hours, function(d) {
        return d3.max(d.taxons, function(dd) {
          if (dd.id === row_id) {
            return dd.duration;
          } else {
            return 0;
          }
        });
      });
      var row_r = d3.scale.linear().domain([0,maximum_row_duration]).range([1, maximum_bubble_radius]);

      var row_intervals = chart_taxon_rows.select('g.taxon-row-'+row_id).selectAll('g.interval').select('rect');

      row_intervals
        .transition()
        .duration(500)
        .attr('height', function(d) { return row_r(d.duration)*2; })
        .attr('y', function(d) { return (row_height/2) - row_r(d.duration); });

    };

    var unfocus_row = function(row_id) {
      var row_intervals = chart_taxon_rows.select('g.taxon-row-'+row_id).selectAll('g.interval').select('rect');
      row_intervals.transition()
        .duration(100)
        .attr('height', function(d) { return r(d.duration)*2; })
        .attr('y', function(d) { return (row_height/2) - r(d.duration); });
    };


    var show_tooltip = function(info, mouse) {
      var tooltip_left = mouse[0] + 25;
      var tooltip_top = mouse[1];

      var time_label = format_hour(info.hour, RTDATA.user_context.hour_format_preference) + " and " + format_hour(info.hour+1, RTDATA.user_context.hour_format_preference);

      var tooltip_message = "This " + RTDATA.report_context.time_frame + ", you logged " + Math.round(info.duration / 60) + " minutes between " + time_label + ".";

      chart_tooltip.select('.context-tooltip-name').style('display', 'none');
      chart_tooltip.select('.context-tooltip-duration').text(tooltip_message);

      chart_tooltip.style('display','block');
      chart_tooltip.style('top', tooltip_top+'px');
      chart_tooltip.style('left', tooltip_left+'px');
    };

    var position_tooltip = function(mouse) {
      var tooltip_left = mouse[0] + 25;
      var tooltip_top = mouse[1];
      chart_tooltip.style('top', tooltip_top+'px');
      chart_tooltip.style('left', tooltip_left+'px');
    };

    var hide_tooltip = function(info) {
      chart_tooltip.style('display','none');
    };

    var get_empty_hours = function() {
      var empty_hours = hours.filter(function(hour) {
        return !(data.hours.some(function(element) { return element.hour === hour;}));
      });
      return empty_hours;
    };
    if (layout_type === 'desktop') {
      var chart_empty_columns = setup_empty_hours();
    }
    var chart_taxon_rows = setup_taxon_rows();
    var chart_tooltip = setup_chart_tooltip();
    setup_interval_labels();
  };



  var browse_changes_over_time = function(element, data, options) {

    if (typeof(options) === 'undefined') {
      options = {};
    }
    element = jQuery(element).get(0);

    $(element).empty();

    if (options.chart_class) {
      jQuery(element).addClass(options.chart_class);
    }
    if (typeof(data) !== 'undefined' && data !== null) {
      data = data;
    } else {
      data = get_dataset(element);
    }

    var padding = [9,200,36,45],
        show_sidebar = true;

    if (options.hide_sidebar) {
      padding = [9,9,36,36];
      show_sidebar = false;
    }

    // this breaks on productivity report by rank!!
    var chart_div = jQuery(element),
        groups_count = 3, // how many overviews will we be displaying
        start_time = iso(data.config.start_date),
        end_time = iso(data.config.end_date),
        height = chart_div.height(),
        width = chart_div.width(),
        maximum_duration = d3.max(data.items, function(d) { return d3.max(d.entries, function(dd) { return dd.duration; }); }),
        minimum_duration = 0,
        max_bar_height = height - padding[0] - padding[2],
        width_with_padding = width - padding[1] - padding[3],
        x = d3.time.scale().domain([start_time, end_time]).range([0, width_with_padding-50]),
        y = d3.scale.linear().domain([0,maximum_duration]).range([0, max_bar_height]),
        //line_colors = d3.scale.category10(),
        y_tick_count = 4,
        bar_colors = d3.scale.category20().domain([0, data.items.length-1]);


    var item_line = d3.svg.line()
      .x(function(d) { return x(iso(d.date)); })
      .y(function(d) { return -y(d.duration); });

    var flat_line = d3.svg.line()
      .x(function(d) { return x(iso(d.date)); })
      .y(function(d) { return -0; });

    // add a tooltip container
    var tooltip = d3.select(element).append("div")
        .attr('class','context-tooltip');
    tooltip.append('div')
      .attr('class', 'context-tooltip-name');
    tooltip.append('div')
      .attr('class', 'context-tooltip-text');
    tooltip.append('div')
      .attr('class', 'context-tooltip-duration');

    // draw the chart svg
    var chart_svg = d3.select(element).append("svg:svg")
        .attr("id", 'dashboard_spotlight_chart')
        .attr("width", width)
        .attr("height", height);

    var chart = chart_svg.append("svg:g")
        .attr("transform", "translate(" + padding[3] +", "+ (height - padding[2]) + ")");

    // draw y axis lines
    chart.selectAll('.yline')
      .data(y.ticks(y_tick_count))
      .enter().append('line')
      .attr('class', 'yline')
      .attr('x1', 0)
      .attr('y1', function(d) { return -y(d); })
      .attr('y2', function(d) { return -y(d); })
      .attr('x2', width_with_padding);


    // draw the baseline
    chart.append('line')
      .attr('class', 'spotlight-baseline')
      .attr('x1', 0)
      .attr('y1', 0)
      .attr('y2', 0)
      .attr('x2', width_with_padding)
      .style('stroke-width', 2)
      .style('stroke', '#444');


    // y axis labels
    chart.selectAll('.ylabel')
      .data(y.ticks(y_tick_count))
      .enter().append('svg:text')
      .attr('class', 'ylabel')
      .text(function(d) {
        if (d === 0) {
          return d;
        } else {
          if (maximum_duration > (45*60)) {
            return Math.round((d/60/60)*10)/10 + 'h';
          } else {
            return Math.round(d/60) + 'm';
          }
        }
      })
      .attr('x', -5)
      .attr('y', function(d) { return -y(d) + 3; })
      .attr('text-anchor', 'end');


    if (show_sidebar) {
      // line labels
      chart_svg.selectAll('.item-label')
        .data(data.items)
        .enter().append('svg:g')
        .attr('class', 'item-label')
        .attr('transform', function(d,i) {
          return "translate(" + (padding[3]+width_with_padding+9) +", "+ (54+padding[0]+(i*36)) + ")";
        })
        .on('mouseover',  function(d,i) {
          chart.select("#item-lines").classed('focused', true);
          chart.select('#item-line-'+i).classed('focused', true);
        })
        .on('mouseout', function(d,i) {
          chart.select("#item-lines").classed('focused', false);
          chart.selectAll('.item-line').classed('focused', false);
        })
        .call(draw_item_label);
    }


    // x axis labels
    var date_labels = data.items[0].entries;
    var labelformat = d3.time.format("%-m/%-e");

    if (data.config.interval === 'day') {
      // thin out the date lables so they'll fit on the axis better
      var thinned_date_labels = [];
      for(var i=0;i<date_labels.length;i+=1) {
        if (i % 2 === 0) {
          thinned_date_labels.push(date_labels[i]);
        }
      }
      date_labels = thinned_date_labels;
    } else if (data.config.interval === 'month') {
      labelformat = d3.time.format("%b");
    }

    chart.selectAll('text.date-label')
      .data(date_labels)
      .enter().append('svg:text')
      .attr('class', 'date-label')
      .attr('y', 18)
      .attr('x', function(d) {
        return x(iso(d.date)) + 25;
      })
      .attr('text-anchor', 'middle')
      .text(function(d) {
        return labelformat(iso(d.date));
      });

    function draw_item_label(selection) {
      selection.append('rect')
        .attr('x', 0)
        .attr('y', 0)
        .attr('rx', 3)
        .attr('ry', 3)
        .attr('width', padding[1]-10)
        .attr('height', 27)
        .style('fill', function(d,i) {
          return chart_item_color(d,i,bar_colors);
        });

      selection.append('svt:text')
        .attr('x', 5)
        .attr('y', 18)
        .text(function(d) {
          return d.name;
        });
    }


    // draw lines on the x axis
    var item_lines = chart.append('svg:g')
      .attr('id', 'item-lines')
      .attr('transform', "translate("+ 25 + ", 0)");

    var line_group = item_lines.selectAll('g.item-line-group')
      .data(data.items)
      .enter().append('svg:g')
        .attr('class', 'item-line-group');

    line_group.append('svg:path')
      .attr('class', 'item-line')
        .attr('id', function(d,i) { return 'item-line-'+ i; })
        .attr('stroke', function(d,i) { return chart_item_color(d,i,bar_colors); })
        .attr('d', function(d) { return flat_line(d.entries); })
        .transition()
        .duration(1000)
        .attr('d', function(d) { return item_line(d.entries); });


    line_group.selectAll('circle')
      .data(function(d) { return d.entries.map(function(point) { point.label = d.name; return point; } ); })
      .enter().append('svg:circle')
        .attr('cx', function(d){return x(iso(d.date));})
        .attr('cy', function(d){return -y(d.duration);})
        .attr('r', 5)
        .attr('fill', '#fbf9ed')
        .attr('opacity', 0)
        .attr('stroke-width', 1)
        .attr('stroke', '#c9cccf')
        .on('mouseover', function(d) {
          d3.select(this).attr('opacity', 1);
          position_context_tooltip(d,d3.mouse(element));
        })
        .on('mouseout', function(d) {
          d3.select(this).attr('opacity', 0);
          hide_context_tooltip();
        });

    // internal function for the time_by_interval dashboard chart. cant be reused elsewhere.
    function position_context_tooltip(d,mouse) {
      var tooltip_left = mouse[0] + 25;
      var tooltip_top = mouse[1];

      var interval_name = interval_full_label(iso(d.date), RTDATA.report_context.time_frame, RTDATA.report_context.time_frame);
      var interval_duration = duration_to_time(d.duration);

      tooltip.select('.context-tooltip-name').text(interval_name);
      tooltip.select('.context-tooltip-duration').text(interval_duration + ' ' + d.label);

      tooltip.style('display','block');
      tooltip.style('top', tooltip_top+'px');
      tooltip.style('left', tooltip_left+'px');
    }

    function hide_context_tooltip() {
      tooltip.style('display','none');
    }

  };

  var inline_sparkline = function(el, options) {

    if (typeof(options) === 'undefined') {
      options = {};
    }
    var element = jQuery(el).get(0);

    $(element).empty();

    // sparkline config
    var height = jQuery(element).height(),
        width = jQuery(element).width(),
        points = JSON.parse(element.dataset.values),
        max_count = d3.max(points),
        x = d3.scale.linear().domain([0,points.length]).range([0,width]),
        y = d3.scale.linear().domain([0,max_count]).range([height-2, 0]);

    // draw the chart svg
    var chart_svg = d3.select(element).append("svg:svg")
        .attr("width", width)
        .attr("height", height);


    // draw the line
    var line = d3.svg.line()
      .x(function(d,i) {
        return x(i);
      })
      .y(function(d,i) {
        return 1+y(d);
      })
      .interpolate('cardinal');

    chart_svg.append("svg:path").attr("d", line(points));
  };

  var calendar_bubbles = function(el, options) {

    if (typeof(options) === 'undefined') {
      options = {};
    }
    var element = jQuery(el).get(0);
    var data = get_dataset(element);

    // structural / sizing values
    var height = jQuery(element).height(),
        width = jQuery(element).width(),
        padding = {top:27, left: 9, bottom: 9, right: 9},
        calendar_height = (height-padding.top-padding.bottom),
        calendar_width = (width-padding.left-padding.right),
        week_height = calendar_height/data.weeks.length,
        day_width = calendar_width/7,
        radius_max = (d3.min([day_width, week_height])/2)-4;

    // some utils
    var dateparse = d3.time.format("%Y-%m-%d").parse;
    var dayformat = d3.time.format('%a');
    var dayletter = function(date) { return dayformat(date).charAt(0); };


    // data
    var week_starts = data.weeks.map(function(w) { return dateparse(w.start_date); });
    var legend_weekday_start = week_starts[0];
    var legend_weekday_end = d3.time.day.offset(legend_weekday_start, 7);
    var legend_days = d3.time.days(legend_weekday_start, legend_weekday_end);
    var max_daily_duration = d3.max(data.weeks, function(d) { return d3.max(d.days, function(dd) { return dd.duration; }); });
    var current_date = dateparse(data.current_date);

    if (typeof(max_daily_duration) == 'undefined') {
      max_daily_duration = 0;
    }

    // scales
    var weekday_index_scale = d3.scale.linear().domain([0, 7]).range( [0, calendar_width] );
    var weekly_scale = d3.time.scale().domain([week_starts[0], d3.time.day.offset(week_starts[week_starts.length-1], 7)]).range([0, calendar_height]);
    var radial_scale = d3.scale.linear().domain([0,max_daily_duration]).range([2, radius_max]);

    // draw the chart svg
    var chart_svg = d3.select(element).append("svg:svg")
        .attr("width", width)
        .attr("height", height);

    // draw the weekdays up top;
    var legend_group = chart_svg.append("g")
      .attr('transform', 'translate(' + padding.left + ',9)');

    // draw the weekdays up top;
    var weeks_group = chart_svg.append("g")
      .attr('transform', 'translate(' + padding.left + ',40)');

    var day_labels = legend_group.selectAll('.daylabel').data(legend_days);

    day_labels.enter().append('svg:text');
    day_labels.exit().remove();

    day_labels
      .attr('class', 'daylabel')
      .text(function(d) {
        if (d === 0) {
          return d;
        } else {
          return dayletter(d);
        }
      })
      .attr('x', function(d,i) { return weekday_index_scale(i)+(day_width/2);})
      .attr('y', 0)
      .attr('text-anchor', 'middle');


    var draw_day_markers = function(d,i) {

      var week_data = d,
          week_start_date = dateparse(week_data.start_date),
          week_end_date = d3.time.day.offset(week_start_date, 7);

      var weekday_time_scale = d3.time.scale().domain([week_start_date, week_end_date]).range( [0, calendar_width] );

      var weekdays = d3.select(this).selectAll('.day').data(d.days);

      weekdays.enter().append('circle');
      weekdays.exit().remove();

      weekdays.attr('class', function(dd) {
          classes = ['day'];
          if (dd.duration != null && dd.duration > (max_daily_duration/50)) {
            classes.push('with-duration');
          }
          return classes.join(' ');
        })
        .attr('cx', function(dd,i) {
          return weekday_time_scale(dateparse(dd.date))+(day_width/2);
        })
        .attr('cy', 0)
        .attr('r', function(dd,i) {
          if (dd.duration != null && dd.duration > (max_daily_duration/50)) {
            return radial_scale(dd.duration);
          } else {
            return 2;
          }
        });

      // can we detect the current date in this block?
      // for (var i = d.days.length - 1; i >= 0; i--) {
      //   if (dateparse(d.days[i].date).getTime() == current_date.getTime()) {
