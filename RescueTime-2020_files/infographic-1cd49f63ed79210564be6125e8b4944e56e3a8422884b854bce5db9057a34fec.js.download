RT.config_settings = (function(){

  // attach some behaviors to the config item edit forms.
  // this might be better off in a "goals.js" file
  $(document).on('click', 'DIV.config-item A.config-edit-item-link', function(e) {
    $(this).parents('DIV.config-item').addClass('editing');
    e.preventDefault();
  });
  $(document).on('click', 'DIV.config-item A.config-delete-item-link', function(e) {
    $(this).parents('DIV.config-item').addClass('deleting');
    e.preventDefault();
  });

  $(document).on('click', 'DIV.config-item A.config-edit-item-cancel-link', function(e) {
    $(this).parents('DIV.config-item').removeClass('editing deleting submitting');
    e.preventDefault();
  });

  // add a class called 'submitting' to the item when the form is submitted
  $(document).on('ajax:beforeSend', 'DIV.config-item-edit FORM', function(event) {
    $(this).parents('DIV.config-item').addClass('submitting');
  });

  // when the edit form returns, reset to the initial view state
  $(document).on('ajax:success', 'DIV.config-item-edit FORM', function(event, data, status, xhr) {
  	var parent = $(this).parents('DIV.config-item');
  	parent.removeClass('editing deleting submitting');
  });

  // toggle between dynamic and standard goal types
  $(document).on('click', 'A.goalert-type-switch', function(e) {
    var is_dynamic = $(this).data('dynamic'),
        parent = $(this).parents('DIV.config-item'),
        span = $(this).find('SPAN'),
        input = parent.find('INPUT.is-dynamic-input');

    if (is_dynamic) {
      span.text('dynamic');
      parent.removeClass('is_dynamic');
      input.val(false);
    } else {
      span.text('fixed');
      parent.addClass('is_dynamic');
      input.val(true);
    }
    $(this).data('dynamic', !is_dynamic);

    e.preventDefault();
  });

  $(document).on('click', 'A.goalert-type-selector', function(e) {
    $(this).addClass('selected');
    $(this).siblings('A.goalert-type-selector').removeClass('selected');

    var item = $(this).parents('DIV.reveal').find('DIV.config-item'),
        input = item.find('INPUT.is-dynamic-input');

    if($(this).hasClass('dynamic')){
      item.addClass('is_dynamic');
      input.val(true);
    } else {
      item.removeClass('is_dynamic');
      input.val(false);
    }
    e.preventDefault();
  });

  $(document).on('change', 'DIV.goalert-schedule-edit SELECT', function(e) {
    var form = $(this).parents('FORM').get(0);
    var select = $(form).find('SELECT#alert_is_blocking OPTION[value=-1]');
    if ($(this).val() !== 0) {
      $(select).text('for the rest of the selected time period');
    } else {
      $(select).text('for the rest of the day');
    }
  });

  $(document).on('change', 'DIV.goalert-item-edit INPUT.goalert_seconds_parts' , function(e) {
    var form = $(this).parents('FORM').get(0);

    var amount_hours_field = $(form).find('INPUT#alert_amount_hours_part, INPUT#goal_amount_hours_part');
    var amount_mins_field = $(form).find('INPUT#alert_amount_mins_part, INPUT#goal_amount_mins_part');
