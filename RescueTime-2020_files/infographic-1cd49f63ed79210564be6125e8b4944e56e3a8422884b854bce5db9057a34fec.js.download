	// do a monthly rollup
	var monthly_rollup = function(data, data_point) {
		months = [];

        year_months = d3.time.months(date_parser(RTDATA.start_date), date_parser(RTDATA.end_date));

        // initialize the months so we're protected against empty months
        for (var i = 0; i < year_months.length; i++) {
            var year_month_number = year_months[i].getMonth();
            if (typeof(months[year_month_number]) == 'undefined') {
                months[year_month_number] = {date: year_months[i], duration: 0};
            }
        }

		for (var i = data.length - 1; i >= 0; i--) {
			var date = d3.time.month( date_parser( data[i]['summary_date'] ));
			var month_number = date.getMonth();

			if (typeof(months[month_number]) == 'undefined') {
				months[month_number] = {date: date, duration: 0};
			}

			months[month_number]['duration'] += data[i].duration;
		}

		return months;
    }

    // // don't roll up, but group by month
    var group_by_month = function(data, data_point) {
        months = [];

        year_months = d3.time.months(date_parser(RTDATA.start_date), date_parser(RTDATA.end_date));

        // initialize the months so we're protected against empty months
        for (var i = 0; i < year_months.length; i++) {
            var year_month_number = year_months[i].getMonth();
            if (typeof(months[year_month_number]) == 'undefined') {
                months[year_month_number] = {date: year_months[i], duration: 0, days: [], label: month_format(year_months[i])};
                // populate the days array with zero values
                var days_in_month = d3.time.days(year_months[i], d3.time.month.ceil(d3.time.second.offset(year_months[i], 1)));

                for (var ii = 0; ii < days_in_month.length; ii++) {
                    months[year_month_number]['days'][ii] = {
                        date: days_in_month[ii],
                        duration: 0,
                        color: '#cccccc',
                        day_of_week: days_in_month[ii].getDay()
                    };
                }

            }
        }

        for (var i = data.length - 1; i >= 0; i--) {
			var date = date_parser( data[i]['summary_date'] );
            var month_number = date.getMonth();
            var day_number = date.getDate()-1;

            months[month_number]['days'][day_number]['duration'] += data[i][data_point];

            // assign an appropriate color where we can?
            if (data_point == 'duration') {
                months[month_number]['days'][day_number]['color'] = RT.pulse_color(data[i]['avg_score']);
            } else if (data_point == 'all_productive_duration') {
                months[month_number]['days'][day_number]['color'] = RT.pulse_color(75);
            } else if (data_point == 'all_distracting_duration') {
                months[month_number]['days'][day_number]['color'] = RT.pulse_color(25);
            }
		}

		return months;

    }

