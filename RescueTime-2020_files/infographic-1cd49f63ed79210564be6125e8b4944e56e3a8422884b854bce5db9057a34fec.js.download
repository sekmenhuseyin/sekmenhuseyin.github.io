    e.preventDefault();

    if (jQuery(link).hasClass('report-loaded')) {
      var details_table = jQuery(this).parents('TD').find('TABLE.activity-detail-list');
      if (jQuery(details_table).is(":visible")) {
        jQuery(details_table).hide();
