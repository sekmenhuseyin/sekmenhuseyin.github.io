// fixes for IE11 ES6 functions
if (!Array.prototype.find) {
  Array.prototype.find = function(predicate) {
    if (this == null) {
      throw new TypeError('Array.prototype.find called on null or undefined');
    }
    if (typeof predicate !== 'function') {
      throw new TypeError('predicate must be a function');
    }
    var list = Object(this);
    var length = list.length >>> 0;
    var thisArg = arguments[1];
    var value;

    for (var i = 0; i < length; i++) {
      value = list[i];
      if (predicate.call(thisArg, value, i, list)) {
        return value;
      }
    }
    return undefined;
  };
}

if (!String.prototype.startsWith) {
    String.prototype.startsWith = function(searchString, position){
      position = position || 0;
      return this.substr(position, searchString.length) === searchString;
  };
}

// Some utility functions that are broadly applicable across the RescueTime website.
// Chunk an array into smaller arrays, based on an index
function array_chunk(a, increment) {
  var chunks = [];
  while (a.length > 0) {
    chunks.push(a.splice(0, increment));
  }
  return chunks;
}


// return an ISO formatted string as a date
// in our case, there will never be anything returned from the server where we care about local time zone formatting
// so strip that off and use a modified ISO parser instead
function iso(iso_string) {
  // remove the time zone if ignore_time_zone == true and iso_string is actually a string (dates will pass through)
  // if (typeof(ignore_time_zone) != 'undefined' && ignore_time_zone == true && typeof(iso_string) == 'string') {
  //   iso_string = iso_string.replace(/-\d{2}:\d{2}$/, '');
  //   var parser = d3.time.format('%Y-%m-%dT%H:%M:%S').parse;
  // } else {
  //   var parser = d3.time.format.iso.parse;
  // }

  if (typeof(iso_string) === 'string') {
    iso_string = iso_string.replace(/[-+]\d{2}:\d{2}$/, '');

    // If the time is in UTC, strip the 'Z' off the end
    iso_string = iso_string.replace(/Z$/, '');

      // if the time has ms precision, remove it
    if ( iso_string.indexOf('.') > 0 ) {
      iso_string = iso_string.substring(0, iso_string.indexOf('.'));
    }

    var parser = d3.time.format('%Y-%m-%dT%H:%M:%S').parse;

    var d = parser(iso_string);

    if (d !== null && typeof(d.getYear) === 'function') {
      return d;
    } else {
      throw iso_string + " is not a properly formatted ISO datetime string!";
    }

  } else {
    return iso_string;
  }

}


// Flexible expandable content framework.
// Basically set up a structure like this:
// Container.tappable-item
// - Title.tappable-item-label
// - Content.tappable-item-panel
//   - if content has a data-content-src attribute, an ajax call will try to load the content
var tappableItems = {
  init: function() {

    // set up a couple of events
    jQuery(document).on('click', '.tappable-item-label', function(event) {
      var current_element = jQuery(this).parents('.tappable-item').get(0);
      tappableItems.closeAll(current_element);

      tappableItems.toggleContent(current_element);

      event.stopPropagation();
      event.preventDefault(); // stop it from triggering if it's a link;

      jQuery('body').one('click',function(event) {
        // make sure we're not clicking the label again, which will trigger a redundant "closeAll" call
        if (jQuery(event.target).parents('.tappable-item-label').length === 0) {
          tappableItems.closeAll();
        }
      });
    });

    jQuery(document).on('tap:open', '.tappable-item-panel', function(event) {

      // lazy load any content from the url data-content-src
      var content_src = jQuery(this).data('content-src');
      if (typeof(content_src) !== 'undefined') {
        jQuery(this).append('<div class="tappable-item-loading">loading...</div>');
        jQuery.get(content_src, function(data) {
          jQuery(this).find('.tappable-item-loading').replaceWith(data);
        });
      }

    });


  },
  closeAll: function(el) {
    // sweeper function that can be called externally to close any open popouts
    jQuery('.tappable-item').filter(':visible').each(function() {
      if (typeof(el) === 'undefined' || el !== this) {
        tappableItems.hideContent(this);
      }
    });
  },
  showContent: function(el) {
    // trigger the tap:expand event
    //jQuery(el).find('.tappable-item-panel').show();
    jQuery(el).addClass('tapped');
    jQuery(el).find('.tappable-item-panel').trigger('tap:open');
  },
  hideContent: function(el) {
    //jQuery(el).find('.tappable-item-panel').hide();
    jQuery(el).removeClass('tapped');
    jQuery(el).find('.tappable-item-panel').trigger('tap:close');
  },
  toggleContent: function(el) {
    if (jQuery(el).hasClass('tapped')) {
      tappableItems.hideContent(el);
    } else {
      tappableItems.showContent(el);
    }
  }
};


// Reusable widget that will make the dot navigation pattern easier
var navCycler = function(el, nav_count, click_function) {
  var current_index = 0;
  var element = jQuery(el);

  for (var i=0;i<nav_count;i++) {
    element.append('<span class="cycle-bullet" data-index="'+i+'"></span>');
  }

  var bullets = element.find('SPAN.cycle-bullet');

  if (typeof(click_function) === 'function') {
    bullets.click(click_function);
  }

  // this seems like it should work, but it really causes a stupid infinite loop
  var activate = function(i) {
    if (typeof(i) !== 'undefined') {
      highlight(Number(i));
      jQuery(bullets[Number(i)]).trigger('click');
    }
  };

  var forward = function() {
    activate(current_index < bullets.length-1 ? current_index + 1 : 0);
  };
  var back = function() {
    activate(current_index > 0 ? current_index-1 : bullets.length-1);
  };
  var highlight = function(index) {
    reset();
    current_index = index;
    jQuery(bullets[index]).addClass('selected');
  };
  var reset = function() {
    bullets.removeClass('selected');
  };

  return {
    'forward': forward,
    'back': back,
    'highlight': highlight,
    'bullets': bullets
  };
};


// data bullets
// prepend a small, inline pie chart on any element with the data-pie-bullet-value attribute

var pie_bullets = function() {
  jQuery('*[data-pie-bullet-value]').each(function() {
    var diameter = 18;
    if (jQuery(this).data('pie-bullet-diameter') !== undefined) {
      diameter = Number(jQuery(this).data('pie-bullet-diameter'));
    }
    var radius = diameter/2,
        bg_color = '#f5f0d7',
        fill_color = '#65a870',
        start_angle = 1.5 * Math.PI;

    var bullet_canvas = document.createElement('canvas');
    var bullet_percent = Number(jQuery(this).data('pie-bullet-value'));
    bullet_canvas.height = diameter;
    bullet_canvas.width = diameter;
    if (bullet_canvas.getContext && typeof(bullet_percent) !== null) {
      var bullet_context = bullet_canvas.getContext('2d');

      // draw the background circle
      bullet_context.beginPath();
      bullet_context.arc(radius, radius, radius, 0, 2*Math.PI, false);
      bullet_context.fillStyle = bg_color;
      bullet_context.fill();

      // draw the pie piece
      var degrees = bullet_percent * 360.0;
      var radians = degrees * (Math.PI / 180);
      bullet_context.beginPath();
      bullet_context.moveTo(radius, radius);
      bullet_context.arc(radius, radius, radius, start_angle, radians+start_angle, false);
      bullet_context.fillStyle = fill_color;
      bullet_context.fill();

      jQuery(this).prepend(bullet_canvas);
    }

  });
};


// Simple tab support
// Foundation has a built in way of doing this, but the CSS was failing miserably for me, so I build my own. like a dummy
var init_tabsets = function() {
  jQuery('UL[data-tabset-labels] LI').click(function(e) {
    if (jQuery(this).hasClass('active')) {
      return false;
    }

    var parent = jQuery(this).parents('*[data-tabset]');
    var content_id = jQuery(this).data('content-id');
    if (typeof(parent) !== 'undefined') {
      jQuery(parent).find('UL[data-tabset-labels] LI.active').removeClass('active');
      jQuery(parent).find('*[data-tabset-content] DIV.active').removeClass('active');

      jQuery(parent).find('DIV#'+content_id).addClass('active');
      jQuery(this).addClass('active');
    }
  });

};


// Text that's rotated with CSS (svg or html) looks intolerably crappy on Windows machines
// in a case of ridiculous over-engineering, you can use this function to render a canvas element instead.
// since canvas is pure pixel data, it can be rotated without screwing the text quality
// expects:
// - text: the string to render (on one line)
// - options:
// - - width (default is 150px)
// THIS IS EXPERIMENTAL, AND WE GET FEWER COMPLAINTS ABOUT IT, GOOD CANDIDATE FOR REMOVAL NEXT TIME WE DO @cleanup
var render_text_as_canvas = function(text, options) {
  if (typeof(options) === 'undefined') {
    options = {};
  }

  var width = options.width || 150,
      height = options.height || 18,
      color = options.color || '#000',
      font = options.font || '8pt Helvetica',
      align = options.align || 'left',
      start_point = 0,
      rotate = 315,
      rotate_origin = "0% 50%";

  if (align === 'right') {
    start_point = width;
    rotate_origin = "100% 50%";
  } else if (align === 'center') {
    start_point = width/2;
    rotate_origin = "50% 50%";
  }

  if (typeof(text) === 'undefined') {
    return false;
  } else {

    if (text.length > 25) {
      text = text.substr(0,22) + '...';
    }

     var css = [];
     if (rotate !== 0) {
       css.push('transform-origin: '+ rotate_origin + ';');
       css.push('-webkit-transform-origin: '+ rotate_origin + ';');
       css.push('-ms-transform-origin: '+ rotate_origin + ';');
       css.push('transform: rotate('+ rotate + 'deg);');
       css.push('-webkit-transform: rotate('+ rotate + 'deg);');
       css.push('-moz-transform: rotate('+ rotate + 'deg);');
       css.push('-ms-transform: rotate('+ rotate + 'deg);');

       if (align === 'right') {
         css.push('position: absolute;');
         css.push('left: '+-(width-height)+'px;');
       }
     }

     var canvas_el = jQuery('<canvas height="'+height+'" width="'+width+'" style="'+ css.join(' ') +'"></canvas>').get(0);
     var ctx = canvas_el.getContext("2d");

     ctx.fillStyle = color;
     ctx.font = font;
     ctx.textAlign = align;
     ctx.textBaseline = "middle";
     ctx.fillText(text, start_point , canvas_el.height / 2);

     return canvas_el;

  }
};

// format a number of seconds to a readable format.
function duration_to_time(seconds) {
  var hours = Math.floor(seconds / 60 / 60);
  var minutes = Math.floor((seconds - (hours * 60 * 60)) / 60);
  if (hours === 0) {
    if (minutes === 0) {
      return seconds+'s';
    } else {
      return minutes+'m';
    }
  } else if (hours > 99) {
    return hours+'h';
  } else {
    return hours+'h '+ minutes+'m';
  }
}


// format an hour label and respect the users 24 hour preference
function format_hour(hour, hour_format_preference) {
  var time_label = '';
  if (hour_format_preference === 24) {
    time_label = hour+ ":00";
  } else if (hour === 0) {
    time_label = 12;
  } else if (hour === 12) {
    time_label = 'noon';
  } else if (hour > 12) {
    time_label = (hour-12) + 'pm';
  } else {
    time_label = hour + 'am';
