      // setting these every time it draws it causing some weird flickering.
      // test for a difference before updating the dom nodes.
      if (tooltip_text_has_been_updated === true) {
        tooltip.select('.interval-tooltip-name').text(tooltip_name);
        tooltip.select('.interval-tooltip-duration').html('<div>'+tooltip_detail_duration+'</div><div class="tooltip-total-duration">' + tooltip_total_duration + '</div>');

        tooltip_text_has_been_updated = false;
