// get query string params, return as an array
// you can pass in an array of keys to filter on if you need to filter the array
// You may want to filter the array if you were generating a url that updated a specific value.
function query_string_parameters(only_keys) {
  if (typeof(only_keys) === 'undefined') {
    only_keys = [];
  }
  var re = /([^&=]+)=?([^&]*)/g;
  var decodeRE = /\+/g;  // Regex for replacing addition symbol with a space
  var decode = function (str) {return decodeURIComponent( str.replace(decodeRE, " ") );};

  var query = document.location.search.replace('?', '');

  var params = {},e;

  while ( e = re.exec(query) ) {
      var k = decode( e[1] ), v = decode( e[2] );
      // if only_keys defined, make sure the key is a match
      if (only_keys.length === 0 || typeof(only_keys.find(function(key) { return k.match(key) !== null; } ) ) !== 'undefined'  ) {
        if (k.substring(k.length - 2) === '[]') {
            k = k.substring(0, k.length - 2);
            (params[k] || (params[k] = [])).push(v);
        }
        else params[k] = v;
      }
  }
  return params;
}

