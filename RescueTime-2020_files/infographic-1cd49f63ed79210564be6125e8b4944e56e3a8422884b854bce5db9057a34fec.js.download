  // schedule UI utility

  function validateScheduleTimes(times){
    var $scheduleTimes = $(times),
        currStartHour,
        currEndHour,
        prevEndHour,
        badStartOpts,
        badEndOpts,
        midnightBlockIndex = null;

    // clear the board
    $scheduleTimes.find('OPTION').show();

    toggleAddLink($scheduleTimes);

    $scheduleTimes.each(function(i,timeBlock){
      var $scheduleTime = $(timeBlock);
      currEndHour = parseInt($scheduleTime.find('SELECT.end-hour').val());
      currStartHour = parseInt($scheduleTime.find('SELECT.start-hour').val());

      // remove extra time blocks if currEndHour >= 23
      if(midnightBlockIndex === null){
        if(currEndHour >= 23) {
          midnightBlockIndex = i;
        }
      } else if (i > midnightBlockIndex){
        $scheduleTime.remove();
      }

      // remove extra time blocks if prevEndHour >= 23
      if(prevEndHour >= 23){
        $scheduleTime.remove();
      }

      // not the first block
      if( i > 0 ) {
        // hide invalid start hours for the current block
        badStartOpts = $scheduleTime.find('SELECT.start-hour OPTION').filter(function(){
          return parseInt($(this).val()) <= prevEndHour;
        });
        $(badStartOpts).hide();

        // set the current start_hour to be greater than the last end hour
        if(currStartHour <= prevEndHour){
          $scheduleTime.find('SELECT.start-hour').val( Math.min((prevEndHour + 1), 23) );
        }
      }

      // reset currStartHour
      currStartHour = parseInt($scheduleTime.find('SELECT.start-hour').val());

      // hide invalid end hours within the current block
      badEndOpts = $scheduleTime.find('SELECT.end-hour OPTION').filter(function(){
        return parseInt($(this).val()) <= currStartHour;
      });
      $(badEndOpts).hide();

      // set the current end hour to be greater than current start hour
      if( currEndHour <= currStartHour ) {
        $scheduleTime.find('SELECT.end-hour').val( Math.min((currStartHour + 1), 24) );
      }

      prevEndHour = parseInt($scheduleTime.find('SELECT.end-hour').val());
    });
  }

