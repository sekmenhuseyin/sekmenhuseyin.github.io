        return interval_short_label(d, RTDATA.report_context.interval, RTDATA.report_context.time_frame);
      })
      .attr('x', function(d) { return x(d) + interval_offset; })
      .attr('y', '-9px')
      .attr('text-anchor', 'middle');


    // DRAW Y AXIS LABELS
    // TODO: Revisit to get a better tick alignment so we don't have such odd tick values.
    var y_ticks = y.ticks(2);
    var y_format = function(d) {
      return Math.round((d/60/60)*10)/10+'h';
    };
    var y_label_wrapper = chart.append('svg:g').attr('class', 'label-wrapper');

    // draw the labels
    var ylabels = y_label_wrapper.selectAll('.ylabel').data(y_ticks);

    ylabels.enter().append('svg:text');
    ylabels.exit().remove();

    ylabels
      .attr('class', 'ylabel')
      .text(function(d) {
        if (d === 0) {
          return d;
        } else {
          return y_format(d);
        }
      })
      .attr('x', -15)
      .attr('y', function(d) { return (-y(d)+3); })
      .attr('text-anchor', 'end');

    // draw y axis lines
    var ylines = y_label_wrapper.selectAll('.yline').data(y_ticks);

    ylines.enter().append('line');

    ylines.exit().remove();

    ylines.attr('class', 'yline')
      .attr('x1', 0)
      .attr('y1', function(d) { return -y(d); })
      .attr('y2', function(d) { return -y(d); })
      .attr('x2', width_with_padding)
      .attr('stroke', '#ccc');

    jQuery(window).one('resizeDraw', function() {
      compare_time_series(element);
    });

    var series_paths = {};

    // calculate the paths for the chart

    // this is just terrible, but re-order the data array so the selected source is at the end
    data.sort(function(a,b) { return a.id === selected_series ? 1 : -1; });


    data.forEach(function(series) {
      series_paths[series.id] = intervals.map(function(interval) {
        // we need to understand if this interval exists in the series
        var datapoint = series.rows.filter(function(row){ return iso(row.interval).getTime() === interval.getTime(); });
        var interval_duration = 0;
        if (datapoint.length > 0) {
          interval_duration = datapoint[0].duration;
        }
        return [x(interval) + interval_offset,-y(interval_duration)];
      });

      var series_array_area = d3.merge([[[0,0]], series_paths[series.id], [[x(end_time),0]]]);

      chart.selectAll("path.series-area.series-"+series.id)
        .data([series_array_area])
        .enter().append("path")
          .attr('class', function() {
            var classes = ['series-area', 'series-'+series.id];

            if (series.id === selected_series) {
              classes.push('selected');
            }

            return classes.join(' ');
          })
          .attr("d", d3.svg.area().interpolate('linear'));

      if (series.id === selected_series) {
        // chart.selectAll("circle.series-bullet.series-"+series.id)
        //   .data(series_paths[series.id].filter(function(p) { return p[1] !== 0; }))
        //   .enter().append('circle')
        //     .attr('class', function() {
        //       var classes = ['series-bullet', 'series-'+series.id];

        //       if (series.id === selected_series) {
        //         classes.push('selected');
        //       }

        //       return classes.join(' ');
        //     })
        //     .attr('cx', function(d){ return d[0]; })
        //     .attr('cy', function(d){ return d[1]; })
        //     .attr('r', 5)
        //     .on("mouseover", function(d,i) {
        //       var time_label = interval_full_label(x.invert(d[0]), RTDATA.report_context.interval, RTDATA.report_context.time_frame);
        //       populate_series_interval_tooltip(time_label + " - " + series.name, Math.round(y.invert(-d[1])) );
        //     })
        //     .on("mousemove", function(d,i) {
        //       position_series_interval_tooltip(d,d3.mouse(element));
        //     })
        //     .on("mouseout", function() {
        //       hide_series_interval_tooltip();
        //     });
      }


    });

    // draw the baseline
    chart.append('line')
      .attr('class', 'series-interval-baseline')
      .attr('x1', 0)
      .attr('y1', 0)
      .attr('y2', 0)
      .attr('x2', width_with_padding)
      .style('stroke-width', 2)
      .style('stroke', '#444');

    // internal function for the interval_series_chart chart. cant be reused elsewhere.
    function position_series_interval_tooltip(d,mouse) {
      var tooltip_left = mouse[0] + 25;
      var tooltip_top = mouse[1];

      // setting these every time it draws it causing some weird flickering.
      // test for a difference before updating the dom nodes.
      if (tooltip.select('.interval-tooltip-duration').text() !== tooltip_name) {
        tooltip.select('.interval-tooltip-name').text(tooltip_name);
        tooltip.select('.interval-tooltip-duration').text(tooltip_duration);
      }

      tooltip.style('display','block');
      tooltip.style('top', tooltip_top+'px');
      tooltip.style('left', tooltip_left+'px');
    }

    // popuplate the content of the tooltip
    var populate_series_interval_tooltip = function(name, duration) {
      tooltip_name = name;
      tooltip_duration = duration_to_time(duration);
    };

    function hide_series_interval_tooltip() {
      tooltip.style('display','none');
    }

  };


  /* ==== DAILY PATTERNS CHART ==== */
  // ===================================
  // ===================================
  // ===================================
  // ===================================
  // ===================================

  var browse_daily_patterns = function(element, data, options) {
    var params = {};
    if (typeof(options) === 'undefined') {
      options = {};
    }
    element = jQuery(element).get(0);
    if (options.chart_class) {
      jQuery(element).addClass(options.chart_class);
    }
    if (typeof(data) !== 'undefined' && data !== null) {
      data = data;
    } else {
      data = get_dataset(element);
    }

    var padding = [9,9,36,200],
        show_sidebar = true;

    if (options.hide_sidebar) {
      padding = [9,9,36,9];
      show_sidebar = false;
    }

    var chart_div = jQuery(element),
        groups_count = 3, // how many taxons will we be displaying
        start_hour = data.hours[0].hour,
        end_hour = data.hours[data.hours.length-1].hour,
        height = chart_div.height(),
        width = chart_div.width(),
        maximum_duration = d3.max(data.hours, function(d) { return d3.max(d.taxons, function(dd) { return dd.duration; }); }),
        minimum_duration = 0,
        max_bar_height = height - padding[0] - padding[2],
        width_with_padding = width - padding[1] - padding[3],
        x = d3.scale.linear().domain([start_hour, end_hour+1]).range([0, width_with_padding]),
        y = d3.scale.linear().domain([0,maximum_duration]).range([0, max_bar_height]),
        total_hours = (data.hours[data.hours.length-1].hour+1) - data.hours[0].hour,
        group_width = width_with_padding / total_hours, // get the group width
        group_width_with_padding = group_width * 0.8,
        item_colors = [],
        bar_width = group_width_with_padding / data.config.taxons.length,
        bar_colors = d3.scale.category20().domain([0, data.config.taxons.length-1]);


    // draw the chart svg
    var chart_svg = d3.select(element).append("svg:svg")
        .attr("id", 'dashboard_spotlight_chart')
        .attr("width", width)
        .attr("height", height);

    var chart = chart_svg.append("svg:g")
        .attr("transform", "translate(" + padding[3] +", "+ (height - padding[2]) + ")");

    // draw some bars to represent the hours so the groupings are more obvious
    var hour_bar_data = d3.range(start_hour, end_hour+1);
    chart.selectAll('.hour-background')
      .data(hour_bar_data)
      .enter()
      .append('svg:rect')
      .attr('class', 'hour-background')
      .attr('height', height)
      .attr('y', 0)
      .attr('x', function(d) { return x(d) + group_width * 0.1; })
      .attr('width', group_width_with_padding)
      .attr('height', 9);

    // draw the time labels
    var x_tick_count = hour_bar_data.length;
    if (hour_bar_data.length > 10) {
      x_tick_count = 10;
    }

    var x_ticks = chart.selectAll('text.date-label')
      .data(x.ticks(x_tick_count))
      .enter().append('svg:text')
      .attr('class', 'date-label')
      .attr('y', 27)
      .attr('x', function(d) { return x(d) + (group_width/2);})
      .attr('text-anchor', 'middle')
      .text(function(d) {
        if (RTDATA.user_context.hour_format_preference === 24) {
          return d+ ":00";
        } else if (d === 0) {
          return 12;
        } else if (d === 12) {
          return 'noon';
        } else if (d > 12) {
          return (d-12) + 'pm';
        } else {
          return d + 'am';
        }
      });

    // draw the item labels
    if (show_sidebar) {

      var draw_item_label = function(selection) {
        selection.append('rect')
          .attr('x', 0)
          .attr('y', 0)
          .attr('rx', 3)
          .attr('ry', 3)
          .attr('width', padding[3]-10)
          .attr('height', 27)
          .style('fill', function(d,i) {
            return chart_item_color(d,i,bar_colors);
          });

        selection.append('svt:text')
          .attr('x', 9)
          .attr('y', 18)
          .text(function(d) {
            return d.name;
          });

        selection.on('mouseover', function(d) {
            chart.selectAll('.hour-bar-groups').classed('focused', true);
            chart.selectAll('.hour-bar').classed('focused', false);
            chart.selectAll('.hour-bar-'+d.id).classed('focused', true);
          })
          .on('mouseout', function(d) {
            chart.selectAll('.hour-bar-groups').classed('focused', false);
            chart.selectAll('.hour-bar').classed('focused', false);
          });
      };

      chart_svg.selectAll('.item-label')
        .data(data.config.taxons)
        .enter().append('svg:g')
        .attr('class', 'item-label')
        .attr('transform', function(d,i) {
          return "translate(0, "+ (54+padding[0]+(i*36)) + ")";
        })
        .call(draw_item_label);
    }


    // draw grouped bar chart showing patterns
    var hour_groups = chart.append('svg:g')
      .attr('class', 'hour-bar-groups');

    hour_groups.selectAll('.hour-bar-group')
      .data(data.hours).enter()
      .append('svg:g')
      .attr('class','hour-bar-group')
      .attr('transform', function(d) {
        return "translate( "+ (x(d.hour)+(group_width*0.1)) +", 0 )";
      }).each(build_group_bars);

    function build_group_bars(d,i) {
      var group = d3.select(this);

      group.selectAll('.hour-bar')
        .data(d.taxons).enter()
        .append('svg:rect')
        .attr('x', function(dd,i) {
          return bar_width*i;
        })
        .attr('y', 0)
        .attr('height', 0)
        .attr('width', bar_width-1)
        .attr('class', function(dd) {
          return 'hour-bar hour-bar-' + dd.id;
        })
        .style('fill', function(dd,di) {
          for(var ii=0;ii<data.config.taxons.length;ii+=1) {
