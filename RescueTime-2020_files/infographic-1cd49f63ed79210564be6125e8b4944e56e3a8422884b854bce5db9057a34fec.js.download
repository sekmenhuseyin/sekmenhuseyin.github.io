    var ymax = d3.max( data.map( function(d) { return d3.max( d.rows.map(function(row) { return row.duration; }) ) || 0; }) );

    // unless we're dealing with VERY small amounts of time, round everything to the nearest hour.
    if (ymax > 1800) {
      ymax = Math.ceil(ymax/3600) * 3600;
    }

    var x = d3.time.scale().domain([start_time, end_time] ).range([0, bar_area_width]),
        y = d3.scale.linear().range([0, bar_area_height]).domain([0, ymax]);

    var svg = d3.select(element).append("svg:svg")
