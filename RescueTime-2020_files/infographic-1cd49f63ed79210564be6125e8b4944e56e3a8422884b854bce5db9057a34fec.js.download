  /* ==== STACKED INTERVAL CHART ==== */

  var browse_stacked_interval_chart = function(element) {

    // now expects data to an array of arrays [ [obj,obj,obj], [obj,obj,obj] ]
    var params = init_barchart_params(element, 'interval');
    var stacked_data_groups = [],
        layer_groups = [],
        layers = null,
        stack = null,
        max_y = 0,
        view_mode = 'duration'; // can be 'duration' or 'percentage'

    // if there's no data for the chart. bail out.
    if (params === false) {
      return false;
    }

    // is this a productivity chart? If so, include pulses?
    params.pulse_data = null;
    if (typeof(RTDATA.chart_data.pulses[params.data_id]) !== undefined ) {
      params.pulse_data = RTDATA.chart_data.pulses[params.data_id];
    }

    stack = d3.layout.stack()
      .values(function(d) { return d.intervals; })
      .y(function(d) { return d.duration; })
      .offset('zero')
      .order('default');

    function build_chart() {
      prepare_normalized_bars();
      draw_mode_switcher('duration');
      draw_labels(params);
      build_stacked_bars(params, layer_groups);
      draw_baseline(params);

      draw_schedule_intervals(params);
    }

    function prepare_expanded_bars() {
      params.chart_type = 'percentage';
      params.scales.y = params.scales.y_stacked_percentage;
      stack.offset('expand');
      remove_pulses();
      prepare_stack_data();
    }

    function prepare_collapsed_bars() {
      params.chart_type = 'pulse';
      params.scales.y = params.scales.y_stacked_percentage;
      stack.offset('expand');
      build_pulses(params.pulse_data);
      prepare_stack_data();
    }

    function prepare_normalized_bars() {
      params.chart_type = 'interval';
      params.scales.y = params.scales.y_default;
      stack.offset('zero');
      remove_pulses();
      prepare_stack_data();
    }

    function prepare_stack_data() {
      // need to fill each possible interval slot with a value. This is a pain.
      if (stacked_data_groups.length === 0) {
        stacked_data_groups = params.data.map(function(data_group) {

          var stacked_data = data_group.map(function(item){
            var intervals_with_data = {};

            item.intervals.forEach(function(row) {
              intervals_with_data[iso(row.interval).getTime()] = row;
            });

            var complete_intervals = params.intervals.map(function(interval) {
              var duration = 0;
              if (intervals_with_data[interval.getTime()]) {
                duration = intervals_with_data[interval.getTime()].duration;
              }
              return {interval: interval, duration: duration, display_name: item.display_name};
            });

            return {
              'score': item.score,
              'display_name': item.display_name,
              'intervals': complete_intervals
            };
          });

          return stacked_data;
        });

        // ugh, this part is just terrible. loop through all elements and remove any intervals where EVERY item is null
        $.each(stacked_data_groups, function(x,stacked_data){
          $.each(params.intervals, function(y,interval){
            var interval_populated = false;
            $.each(stacked_data, function(z, row){
              if (row.intervals[y].duration !== 0) {
                interval_populated = true;
              }
            });

            if (interval_populated === false) {
              $.each(stacked_data, function(z, row){
                row.intervals[y] = null;
              });
            }
          });

          $.each(stacked_data, function(z, row){
            row.intervals = row.intervals.filter(function(interval) { return interval !== null; });
          });
        });
      }

      layer_groups = stacked_data_groups.map(function(stacked_data){
        return stack(stacked_data);
      });

      if (max_y <= 1) {
        max_y = d3.max( layer_groups.map(function(layers){
          return d3.max(layers[layers.length-1].intervals, function(d) { return d.y0 + d.y; });
        }));
      }

      params.scales.y_default.domain([0,max_y]);
    }


    var update_view_mode = function(mode) {
      if (mode === 'pulse') {
        prepare_collapsed_bars();
        draw_labels(params);
      } else if (mode === 'duration') {
        prepare_normalized_bars();
        draw_labels(params);
      } else {
        prepare_expanded_bars();
        draw_labels(params);
      }

      update_stacked_bars(params, stacked_data_groups);

      params.switcher_buttons.classed('selected', function(d) { return mode === d; });
    };

    var draw_mode_switcher = function(mode) {
      var modes = ['duration','percentage'];

      // if this view includes productivity pulses, do that.
      if (params.pulse_data != null) {
        modes.push('pulse');
      }

      var switcher_buttons_wrapper = params.svg.append('g')
        .attr('class', 'view-mode-switcher')
        .attr('transform', 'translate(' + (params.width - 1) + ',0)')
          .selectAll('rect')
        .data(modes);

      var draw_switcher_button = function(d,i) {
        var button = d3.select(this);
        button.append('svg:rect')
          .attr('x', 0)
          .attr('y', 0)
          .attr('height', '22px')
          .attr('width', '22px');

        button.append('svg:image')
          .attr('xlink:href', precompiled_assets['chart-' + d + '-button'])
          .attr('x', 2)
          .attr('y', 2)
          .attr('height', '18px')
          .attr('width', '18px');
      };

      params.switcher_buttons = switcher_buttons_wrapper.enter().append('svg:g')
          .attr('class', 'switcher-button')
          .attr('id', function(d) { return 'switcher-button-'+d; })
          .attr('transform', function(d,i) {
            return 'translate(' + -((i+1)*28) + ',1)';
          })
          .on('click', function(d) {
            update_view_mode(d);
          })
          .each(draw_switcher_button);

      params.svg.select('g#switcher-button-duration').classed('selected', true);
    };

    // calculate productivity pulses for each interval
    var build_pulses = function(pulses) {
      if (params.pulse_data == null){
        return false;
      }

      var pulse_wrapper = params.chart.append('svg:g').attr('class', 'pulse-wrapper').attr('transform', 'translate(0, ' + params.bar_area_height + ')');

      var pulse_items = pulse_wrapper.selectAll('.pulse-item').data(pulses);

      pulse_items.enter().append('circle');

      pulse_items.exit().remove();

      pulse_items.attr('class', 'pulse-item')
        .attr('cy', function(d) { return -params.scales.y(d.pulse/100); })
            .attr('cx', function(d) { return (params.scales.x_interval(iso(d.interval)) + (params.scales.x1.rangeBand()/2)); })
            .attr('r', d3.min([params.scales.x1.rangeBand()/2, 7]))
            .attr('fill', function(d) { return RT.pulse_color(d.pulse); })
            .on("mouseover",function(d,i){
              update_barchart_tooltip(d,params);
            })
            .on("mousemove", function(d,i) {
              position_barchart_tooltip(d,params);
            })
            .on("mouseout", function() {
              hide_barchart_tooltip(params.tooltip);
            });

      params.chart.selectAll('g.interval-group').style('display', 'none');
    };

    // remove pulses
    var remove_pulses = function() {
      if (params.pulse_data == null){
        return false;
      }
      var pulse_items = params.chart.selectAll('.pulse-item');
      pulse_items.remove();
      params.chart.selectAll('g.interval-group').style('display', 'block');
    };

    $(window).on('resizeDraw', function() {
      console.log('redrawing stacked interval chart');
      $(element).data('rendered', false);
      browse_stacked_interval_chart(element);
    });

    build_chart();

    console.log('DRAW THE INTERVAL BAR CHART');
  };

  var build_stacked_bars = function(params, layer_groups) {
    var stack_colors = d3.scale.category20().domain([0, params.num_items-1]);

    // Add a group for each item.
    var groups = params.chart.append('g').selectAll('g')
                      .data(layer_groups)
                      .enter().append('g')
                        .attr('class', 'interval-group')
                        .attr('transform', function(d, i) { return 'translate(' + params.scales.x1(i) + ', ' + params.bar_area_height + ')'; });

    var items = groups.selectAll('g')
                      .data(function(d){ return d; })
                      .enter().append("svg:g")
                        .attr("class", "interval-items")
                        .style("fill", function(d, i) {
                          var color = chart_item_color(d,i, stack_colors);
                          d3.select(this).attr('data-color', color);
                          return color;
                        });

    // Add a rect for each date.
    items.selectAll("rect")
          .data(function(d) { return d.intervals; })
          .enter().append("svg:rect")
            .attr("x", function(d,i) { return params.scales.x0(iso(d.interval)); })
            .attr("y", 0)
            .attr("height", 0)
            .attr("width", params.scales.x1.rangeBand())
            .on("mouseover",function(d,i){
              update_barchart_tooltip(d,params);
              var color = d3.select(this.parentNode).attr('data-color');
              d3.select(this).style('fill',function(d,i){
                return d3.hsl(color).brighter(0.2);
              });
            })
            .on("mousemove", function(d,i) {
              position_barchart_tooltip(d,params);
            })
            .on("mouseout", function() {
              hide_barchart_tooltip(params.tooltip);
              var color = d3.select(this.parentNode).attr('data-color');
              d3.select(this).style('fill', color);
            });

    var draw_stacked_labels = function(params) {
      var label_data = params.data[0].reverse(); // only need first group's labels

      var item_labels = d3.select(params.element).append('ul').attr('class',function() {
          var classes = ['stacked-labels'];
          return classes.join(' ');
        }).selectAll('li.stacked-label').data(label_data);

      item_labels
        .enter()
        .append('li')
        .style('width', 100/label_data.length + '%')
        .attr('class', 'stacked-label')
        .append('span')
        .style('background', function(d,i) {
          return chart_item_color(d,(label_data.length-1)-i, stack_colors);
        })
        .style('border-color', function(d,i) {
          return chart_item_color(d,(label_data.length-1)-i, stack_colors);
        })
        .on('mouseover', function(d) {

        })
        .text(function(d) { return d.display_name; });
    };

    update_stacked_bars(params, layer_groups);
    draw_stacked_labels(params);
  };

  var update_stacked_bars = function(params, layer_groups) {
    var items = params.chart.selectAll("g.interval-items");

    items.data(layer_groups);

    items.selectAll("rect")
      .transition()
      .duration(params.animation_duration)
      .attr("y", function(d,i) {
        return -params.scales.y(d.y0) - params.scales.y(d.y);
      })
      .attr("height", function(d) { return params.scales.y(d.y) + 'px'; });
  };


