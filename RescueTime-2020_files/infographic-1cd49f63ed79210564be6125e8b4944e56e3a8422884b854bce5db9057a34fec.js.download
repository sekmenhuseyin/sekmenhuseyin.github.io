RT.time_frame_boundaries = function(d) {
  var start,
      end;
  if (RTDATA.report_context) {
    if (typeof(d) === 'undefined') {
      if (RTDATA.report_context.start_date !== undefined && RTDATA.report_context.start_date !== undefined) {
        start = iso(RTDATA.report_context.start_date);
        end = d3.time.day.ceil(d3.time.second.offset(iso(RTDATA.report_context.end_date), 1));

        return [start,end];
      } else {
        d = RTDATA.report_context.date;
      }
    }
    // take the current date and add one second to it so all the incrementing rounding will work
    var date = d3.time.second.offset(iso(d), 1);

    var time_frame = RTDATA.report_context.time_frame;
    if (time_frame === 'day') {
      start = d3.time.day(date);
      end = d3.time.day.ceil(date);
    } else if (time_frame === 'week') {
      if (RTDATA.user_context.week_start_day === 1) {
        start = d3.time.monday(date);
        end = d3.time.monday.ceil(date);
      } else {
        start = d3.time.sunday(date);
        end = d3.time.sunday.ceil(date);
      }
    } else if (time_frame === 'month') {
      start = d3.time.month(date);
      end = d3.time.month.ceil(date);
    } else if (time_frame === 'quarter') {
      // get the quarter number
      var quarter = Math.ceil((date.getMonth())/3)+1; // add one because of zero based month numbers.
      start = new Date(date.getFullYear(), ((quarter-1)*3), 1 );
      end = d3.time.month.ceil(d3.time.month.offset(start, 3));
    } else if (time_frame === 'year') {
      start = d3.time.year(date);
      end = d3.time.year.ceil(date);
    }

    return [start,end];
  } else {
    return null;
  }
};

RT.time_frame_start = function(d) {
  return RT.time_frame_boundaries(d)[0];
};

RT.time_frame_end = function(d) {
  return RT.time_frame_boundaries(d)[1];
};

RT.time_frame_intervals = function(d) {
  if (RTDATA.report_context) {

    var boundaries = RT.time_frame_boundaries(d);
    var interval = RTDATA.report_context.interval;
    if (interval === 'hour') {
      return d3.time.hours(boundaries[0], boundaries[1]);
    } else if (interval === 'day') {
      return d3.time.days(boundaries[0], boundaries[1]);
    } else if (interval === 'week') {
      if (RTDATA.user_context.week_start_day === 1) {
        return d3.time.mondays(boundaries[0], boundaries[1]);
      } else {
        return d3.time.weeks(boundaries[0], boundaries[1]);
      }

    } else if (interval === 'month') {
      return d3.time.months(boundaries[0], boundaries[1]);
    }
  } else {
    return null;
  }
};


RT.comparison_colors = ['#375a98','#f6a623','#007e89', '#48a564','#8c4a00'];

RT.productivity_colors = ['#D61800','#DC685A','#B1C1BF','#3D80E0','#0055C4'];
RT.productivity_colors_by_name = {
  'very distracting': RT.productivity_colors[0],
  'distracting': RT.productivity_colors[1],
  'neutral': RT.productivity_colors[2],
  'productive': RT.productivity_colors[3],
  'very productive': RT.productivity_colors[4]
};

RT.productivity_color = function(score) {
  if (typeof(score) === 'string') {
    return RT.productivity_colors_by_name[score.toLowerCase()];
  } else if (typeof(score) === 'number') {
    // the score is going to map to the order of the productivity colors array, but the indexes need to be adjusted
    return RT.productivity_colors[score+2];
  }
};

// given a productivity pulse value (0-100), compute the cooresponding color
RT.pulse_color = function(pulse) {
  var prod_colors = d3.scale.linear().domain([0,25,50,75,100]).range(RT.productivity_colors.slice(0));
  return prod_colors(pulse);
};

// compare a measure by time period
RT.percent_difference = function(measure, comparison_measure) {
  return Math.round(((Number(measure)-Number(comparison_measure))/Number(comparison_measure))*100);
};

// display an hour value, and respect the user's stated 12/24 hr preference
// possible return values are: 1:23 PM | 13:23
// do we need a way to offer alternate formats? seconds? hours only?
RT.display_time = function(time) {
  var format;
  if (RTDATA.user_context.hour_format_preference !== undefined && RTDATA.user_context.hour_format_preference === 24) {
    format = d3.time.format("%-H:%M");
  } else {
    format = d3.time.format("%-I:%M %p");
  }
  return format(time);
};

// just show the hour (used in some of the chart labels)
RT.display_hour = function(time, skip_ampm) {
  var format;
  if (RTDATA.user_context.hour_format_preference !== undefined && RTDATA.user_context.hour_format_preference === 24) {
    format = d3.time.format("%-H");
  } else if (skip_ampm === true) {
    format = d3.time.format("%-I");
  } else {
    format = d3.time.format("%-I%p");
  }
  return format(time);
};

RT.setup_copy_to_clipboard_links = function() {
  $(document).on('click', 'A.copy-to-clipboard', function(e){
    if($(this).hasClass('pending')){ return; }

    $(this).addClass('pending');
    // get the target within the containing element
    var $link = $(this),
        $target = $link.closest('.clipboard-target');
    // if no target exists in within containing element
    // go up to closest DIV element and search
    if($target.length === 0){
      $target = $link.closest('DIV').find('.clipboard-target');
    }

    if($target.length){
      var originalText = $link.text()

      try {
        $target.select();
        document.execCommand('copy');
        $link.text('copied!');
        setTimeout(function(){
          $link.text(originalText);
          $link.removeClass('pending');
        }, 1000);
      } catch(err) {
        console.log('ERROR: ' + err);
        $link.removeClass('pending');
      }
    } else {
      $link.removeClass('pending');
    }

    e.stopPropagation();
    e.preventDefault();
  });
};

// Resolution should only ever be 'day' or 'hour', I think?
RT.is_time_within_working_hours = function(time, resolution) {
  if (typeof(RTDATA.user_context.working_hours) === 'undefined' || RTDATA.user_context.working_hours == null) {
    // is this just a temporary failsafe?
    return true;
  } else {
    var day = time.getDay(),
        valid_days = RTDATA.user_context.working_hours.map(function(d) { return d.day; });

    // if we're on an invalid day, return false, no matter what the resolution is
    if (valid_days.indexOf(day) === -1) {
      return false;
    } else if (resolution === 'day') {
      // we're on a day within the work hours schedule
      return true;
    } else { // resolution == 'hour'
      // get the matching day of the week from the working hours
      var time_blocks_for_day = RTDATA.user_context.working_hours.filter(function(d) { return d.day === day; });

      if (typeof time_blocks_for_day.length === 0) {
        // another temporary failsafe or '24/7'?
        return true;
      } else {
        var hour = time.getHours(),
            working_hours_for_day = [];

        for(var i in time_blocks_for_day) {
          var block = time_blocks_for_day[i];
          for(var h = block.start_hour; h < block.end_hour; h++){
            working_hours_for_day.push(h);
          }
        }
        return (working_hours_for_day.indexOf(hour) >= 0);
      }
      return true;
    }
  }
};

