      //       .attr('cy', 0)
      //       .attr('cx', weekday_time_scale(dateparse(d.days[i].date))+(day_width/2))
      //       .attr('r', radius_max+1);
      //   }
      // };
    };

    var week_containers = weeks_group.selectAll('.week').data(data.weeks);

    week_containers.enter().append('g');
    week_containers.exit().remove();

    week_containers
      .attr('transform', function(d,i) {
        return 'translate(0,'+ weekly_scale(dateparse(d.start_date)) +')';
      })
      .each(draw_day_markers);


  };

  var dynamic_goal_sparkline = function(el, options) {
    var element = jQuery(el).get(0);

    $(element).empty();

    if (typeof(options) === 'undefined') {
      options = {};
    }


    // sparkline config
    var height = jQuery(element).height(),
        width = jQuery(element).width(),
        points = JSON.parse(element.dataset.values),
        goal_id = element.dataset.id,
        max_count = d3.max(points),
        min_count = d3.min(points),
        circle_radius = 6,
        x = d3.scale.linear().domain([0,(points.length -1)]).range([circle_radius,width-circle_radius]),
        y = d3.scale.linear().domain([min_count,max_count]).range([height-circle_radius, circle_radius]);

    // draw the chart svg
    var chart_svg = d3.select(element).append("svg:svg")
        .attr("width", width)
        .attr("height", height);

    // draw the line
    var line = d3.svg.line()
      .x(function(d,i) {
        return x(i);
      })
      .y(function(d,i) {
        return 1+y(d);
      })
      .interpolate('cardinal');

    var begin_label = d3.select(element).append('div')
        .attr('class', 'sparkline-label begin')
        .html('<span>' + duration_to_time(points[0]) + '</span>')
        .style('left', '-54px')
        .style('top', (parseInt(y(points[0]) - 6) + 'px'));

    var end_label = d3.select(element).append('div')
        .attr('class', 'sparkline-label end')
        .html('<span>' + duration_to_time(points[points.length-1]) + '</span>')
        .style('left', Math.round(x(points.length-1) + 9) + 'px')
        .style('top', (parseInt(y(points[points.length-1]) - 6) + 'px'));

    chart_svg.append("svg:path").attr("d", line(points));

    chart_svg.append('circle')
     .attr('class', 'sparkcircle start')
     .attr('cx', x(0))
     .attr('cy', y(points[0]))
     .attr('r', circle_radius);

    chart_svg.append('circle')
      .attr('class', 'sparkcircle')
      .attr('cx', x(points.length-1))
      .attr('cy', y(points[points.length-1]))
      .attr('r', circle_radius);
  };

