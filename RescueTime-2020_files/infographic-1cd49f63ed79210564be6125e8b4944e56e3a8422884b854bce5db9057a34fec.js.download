  var draw_group_toggles = function(params){

    function redraw_bars() {
      params.data = [];
      group_toggles.each(function(d,i){
        if(!d3.select(this).classed('disabled')){
          params.data.push(params.init_data[i]);
        }
      });

      if(params.data.length === 0) {
        params.data = [params.init_data[params.init_data.length - 1]];
      }

      params.maximum = d3.max(params.data.map(function(d){return d3.max(d, function(dd){return dd.duration;});}));
      params.minimum = d3.min(params.data.map(function(d){return d3.min(d, function(dd){return dd.duration;});}));
      params.scales.y.domain( [0, params.maximum] );
      draw_labels(params);

      params.scales.x1.domain( d3.range( params.data.length ) ).rangeBands( [0, params.group_width], 0.05 );
      params.bar_width = ((params.group_width - params.group_padding) / params.data.length) + params.data.length;
      params.bar_border_radius = params.bar_width < 23 ? 2 : 3;
      draw_bars(params);
    }

    // Clear out for resize
    d3.select('UL.chart-group-toggles').remove();

    params.data = params.data.slice(0,params.data.length - 1); // Remove the default data, in first index
    var group_toggles = d3.select(params.element.parentNode)
                          .append('ul')
                          .attr('class','chart-group-toggles')
                          .selectAll('li.group-toggle')
                          .data(params.data);
    group_toggles
      .enter()
      .append('li')
      .attr('class', 'group-toggle')
      .attr('class', 'disabled')
      .text(function(d) { return d[0].display_name; })
      .style('background', function(d,i) {
        return params.scales.z(i);
      })
      .style('border-color', function(d,i) {
        return params.scales.z(i);
      })
      .on('mouseover', function(d,i) {
        d3.select(this).style('background', d3.hsl(params.scales.z(i)).brighter(0.2));
      })
      .on('mouseout', function(d,i) {
        d3.select(this).style('background', params.scales.z(i));
      })
      .on('click', function(d,i) {
        d3.select(this).classed('disabled', !d3.select(this).classed('disabled'));
        redraw_bars();
      });

      redraw_bars();
  };

  var draw_bars = function(params) {

    function draw_bar(selection) {
      selection.attr('width', params.scales.x1.rangeBand())
        .attr('rx', params.bar_border_radius)
        .attr('ry', params.bar_border_radius)
        .transition()
          .delay(function(d, i) { return i / params.num_items * params.animation_duration; })
          .duration(params.animation_duration)
          .attr('height', function(d) {
            if (params.chart_type === 'percentage') {
              if (d.percentage < 1) {
                return 0;
              } else {
                return params.scales.y(d.percentage / 100);
              }
            } else {
              if (d.duration === 0) {
                return 0;
              } else {
                return params.scales.y(d.duration);
              }
            }
          })
          .attr('y', function(d) {
            if (params.chart_type === 'percentage') {
              return -params.scales.y(d.percentage / 100);
            } else {
              return -params.scales.y(d.duration);
            }
          });
    }

    function pick_bar_color(data,node) {
      var color = null;
      if(data.score === undefined) {
        var group_index = d3.select(node.parentNode).attr('data-group-index');
        color = params.chart_type === 'percentage' ? params.scales.comparison_colors(group_index) : params.scales.z(group_index);
      } else {
        color = RT.productivity_color(data.score);
      }
      return color;
    }

    var bar_groups = params.bar_wrapper.selectAll('g').data(params.data, function(d) { return params.init_data.indexOf(d); });
    bar_groups.enter().append('g');

    bar_groups
      .attr('class', 'xbar')
      .attr('transform', function(d, i) {
        return 'translate(' + params.scales.x1(i) + ', ' + params.bar_area_height + ')';
      })
      .attr('data-group-index', function(d){
        return params.init_data.indexOf(d);
      });

    var rects = bar_groups.selectAll('rect').data(function(d) { return d; });

    // initial dimensions and fill
    rects.enter().append('rect')
      .style('fill', function(d,i) { return pick_bar_color(d,this); })
      .attr('y',0)
      .attr('height',0)
      .attr('x', function(d, i) {
        if (params.chart_type === 'interval') {
          return params.scales.x0(iso(d.interval));
        } else {
          return params.scales.x0(i);
        }
      });

    // pointer events
    rects.on("mouseover",function(d,i){
        update_barchart_tooltip(d,params);
        d3.select(this).style('fill', function(d,i){ return d3.hsl(pick_bar_color(d,this)).brighter(0.2); });
      })
      .on("mousemove", function(d,i) {
        position_barchart_tooltip(d,params);
      })
      .on("mouseout", function() {
        hide_barchart_tooltip(params.tooltip);
        d3.select(this).style('fill', function(d,i) { return pick_bar_color(d,this); });
      })
      .on('click', function(d) {
        if(d.link !== undefined) {
          window.location.href = d.link;
        }
      });

    // class
    rects.attr('class', function(d) {
      // TODO: COLOR PICKER based on score?
      var classes = ['xbar-bar'];
      if(params.chart_type === 'percentage') {
        if (d.percentage === params.maximum * 100) {
          classes.push('maximum-percentage');
        } else if (d.percentage === params.minimum * 100) {
          classes.push('minimum-percentage');
        }
      } else {
        if(params.data.length === 1) {
          classes.push('score'+d.score);
        }
        if (d.duration === params.maximum) {
          classes.push('maximum-duration');
        } else if (d.duration === params.minimum) {
          classes.push('minimum-duration');
        }
      }
      return classes.join(' ');
    });

    // transitions
    rects.call(draw_bar);
    rects.exit().remove();
    bar_groups.exit().remove();
  };

  var position_barchart_tooltip = function(d,params) {
    var mouse = d3.mouse(params.element),
        tooltip_left = mouse[0] + 30,
        tooltip_top = mouse[1] - 20;

    params.tooltip.style('top', tooltip_top+'px');
    params.tooltip.style('left', tooltip_left+'px');
  };

  var update_barchart_tooltip = function(d,params) {
    var tooltip_details = null;

    if(params.chart_type === 'percentage') {
      var formatted_percentage = d3.format("%")(d.y) + '%  of total time';
      tooltip_details = '<div class="tooltip-total-percentage">'+formatted_percentage+'</div>';
      if(d.formatted_duration !== undefined) {
        tooltip_details = '<div class="tooltip-total-duration">'+d.formatted_duration+'</div>' + tooltip_details;
      }
    } else if(params.chart_type === 'pulse') {
      tooltip_details = '<div class="tooltip-pulse">Productivity Pulse: '+d.pulse+'</div>';
      tooltip_details += '<div class="tooltip-total-duration">Total Time Logged: '+duration_to_time(d.duration)+'</div>';
    } else {
      var formatted_duration = duration_to_time(d.duration);
      tooltip_details = '<div class="tooltip-total-duration">'+formatted_duration+'</div>';
    }

    var tooltip_display = "";

    if (params.chart_type === 'interval' || params.chart_type === 'percentage' || params.chart_type === 'pulse' ) {
      tooltip_display = interval_full_label(iso(d.interval), RTDATA.report_context.interval, RTDATA.report_context.time_frame) + '<br>';
    }

    var tooltip_title = params.tooltip.select('.barchart-tooltip-title');
    tooltip_title.html(tooltip_display);

    if (params.chart_type !== 'pulse') {
      // add as text element to prevent XSS
      tooltip_title.append('text').text(d.display_name);
    }

    params.tooltip.select('.barchart-tooltip-details').html(tooltip_details);
    params.tooltip.style('display','block');
  };

  var hide_barchart_tooltip = function(tooltip) {
    tooltip.style('display','none');
  };

  var productivity_color_range = function() {
    return d3.scale.ordinal().range(RT.productivity_colors.slice(0).reverse());
  };

  // labels for time periods that will fit in as small a horizontal place as possible
  var interval_short_label = function(time, interval, time_frame) {
    var day_format = function(d) { return d3.time.format('%a')(d).charAt(0); }, // M, T, W, T, F, S, S
        date_format = function(d) { return d3.time.format('%-d')(d); },  // 1, 2, 3, .... 29, 30, 31
        hour_format = function(d) { return RT.display_hour(d); }, // 1, 2, 3, ... 11, 12, 1, 2, 3, 4, 5, 6...
        week_format = function(d) { return d3.time.format('%-m/%-d')(d); }, // 1/24
        month_format = function(d) { return d3.time.format('%b')(d).charAt(0); };  // J, F, M, A, M, J, J, A, S, O, N, D

    if (interval === 'hour') {
      return hour_format(time);
    } else if (interval === 'day') {
      if (time_frame === 'week') {
        return day_format(time);
      } else {
        return date_format(time);
      }
    } else if (interval === 'week') {
      return week_format(time);
    } else if (interval === 'month') {
      return month_format(time);
    }

  };

  // labels for time periods that will fit in as small a horizontal place as possible
  var interval_full_label = function(time, interval, time_frame) {
    var day_format = function(d) { return d3.time.format('%A')(d);}, // Monday, Tuesday, ... Saturday, Sunday
        date_format = function(d) { return d3.time.format('%b %-d')(d); },  // 1, 2, 3, .... 29, 30, 31
        hour_format = function(d) { return RT.display_hour(d); }, // 1, 2, 3, ... 11, 12, 1, 2, 3, 4, 5, 6...
        week_format = function(d) { return d3.time.format('Week of %-m/%-d')(d); }, // 1/24
        month_format = function(d) { return d3.time.format('%B')(d); };  // J, F, M, A, M, J, J, A, S, O, N, D

    if (interval === 'hour') {
      return hour_format(time);
    } else if (interval === 'day') {
      if (time_frame === 'week') {
        return day_format(time);
      } else {
        return date_format(time);
      }
    } else if (interval === 'week') {
      return week_format(time);
    } else if (interval === 'month') {
      return month_format(time);
    }
  };

  var label_intervals = function(intervals) {

    if (RTDATA.report_context.time_frame === 'day') {
      // if we're looking at a day, return every 3 hours
      return intervals.filter(function(item, index) { return index % 3 === 0; });
    } else if (RTDATA.report_context.time_frame === 'month') {
      // if we're looking at a month, return every 3 days
      return intervals.filter(function(item, index) { return index % 3 === 0; });
    } else if (RTDATA.report_context.time_frame === 'quarter') {
      // if we're looking quarterly, return every, er... 6 days?
      return intervals.filter(function(item, index) { return index % 6 === 0; });
    } else if (RTDATA.report_context.time_frame === 'last30days') {
      return intervals.filter(function(item, index) { return index % 3 === 0; });
    } else if (RTDATA.report_context.time_frame === 'last45days') {
      return intervals.filter(function(item, index) { return index % 5 === 0; });
    } else if (RTDATA.report_context.time_frame === 'last90days') {
      return intervals.filter(function(item, index) { return index % 5 === 0; });
    } else if (RTDATA.report_context.time_frame === 'year') {
      if(RTDATA.report_context.interval === 'week'){
        // Every 5 weeks
        return intervals.filter(function(item, index) { return index % 5 === 0; });
      } else {
        // Every 2 months
        return intervals.filter(function(item, index) { return index % 2 === 0; });
      }

    } else {
      return intervals;
    }
  };

  var productivity_arc = function(el) {


    var element = jQuery(el).get(0);
    var data = get_dataset(element);
    var score = get_data_attribute(element, 'productivity-score');
    var comparison_score = get_data_attribute(element, 'comparison-productivity-score');
    var comparison_label = get_data_attribute(element, 'comparison-label');

    // if there's no data for the chart. bail out.
    if (typeof(element) === 'undefined' || typeof(data) === 'undefined') {
      return false;
    }

    // clear out any existing stuff in this element, in case we're replacing
    jQuery(element).html('');

    var width = jQuery(element).width(),
        height = width,
        radius = width/2,
        inner_radius = radius-34,
        outer_radius = radius-9,
        colors = RT.productivity_colors.slice(0).reverse(), // make a copy of the colors and flip it around.
        outline_arc = d3.svg.arc().outerRadius(radius).innerRadius(inner_radius),
        arc = d3.svg.arc().outerRadius(outer_radius).innerRadius(inner_radius),
        donut = d3.layout.pie().value(function(d){ return d.duration; }).sort(null);

    var chart = d3.select(element).append("svg:svg")
        .attr("class", 'productivity_arc')
        .attr("width", width)
        .attr("height", height)
      .append("svg:g")
        .attr("transform", "translate("+ radius + ","+ radius +")");

    // add an element to hold the hover labels
    var chart_labels = d3.select(element).append('div')
      .attr('class','productivity-arc-labels');

    var chart_hover_tooltip = chart_labels.append('div')
      .attr('class', 'productivity-arc-hover-tooltip')
      .style('display', 'none');

    if (score && comparison_score) {
      var percent_difference = RT.percent_difference(score, comparison_score);
      var comparison_html = '';

      if (percent_difference > 1) {
        comparison_html = "<span class='up-arrow image'>Up</span> "+ percent_difference +"% from " + comparison_label;
      } else if (percent_difference < -1) {
        comparison_html = "<span class='down-arrow image'>Up</span> "+ Math.abs(percent_difference) +"% from " + comparison_label;
      } else {
        comparison_html = "About average";
      }
      var chart_comparison_text = chart_labels.append('div')
        .attr('class', 'productivity-arc-comparison')
        .html(comparison_html);
    }


    // add the filter element for the arc drop shadow
    var filter = chart.append("svg:defs")
      .append("svg:filter")
        .attr("id", "blur")
      .append("svg:feGaussianBlur")
        .attr("stdDeviation", 5);

    var arcs = chart.selectAll("path")
        .data(donut(data))
      .enter().append("svg:path")
        .attr("fill", function(d,i) { return colors[i]; })
        .attr("class", 'slice')
        .on("mouseover", function(d,i) {
          chart_hover_tooltip.text( duration_to_time(d.data.duration) + ' ' + d.data.name );
          chart_hover_tooltip.style('display', 'block');
          d3.select(this).transition().attr('d', outline_arc);
        })
        .on("mouseout", function(d,i) {
          chart_hover_tooltip.text('');
          chart_hover_tooltip.style('display', 'none');
          d3.select(this).transition().attr('d', arc);
        })
        .attr("d", arc);

    var circle_shadow = chart.selectAll('circle.arc-inner-circle-shadow')
      .data([inner_radius-3])
      .enter().append('circle')
        .attr('class', 'arc-inner-circle-shadow')
        .attr('cx', 0)
        .attr('cy', 0)
        .attr('r', String)
        .attr('filter', 'url(#blur)');


    var circle = chart.selectAll('circle.arc-inner-circle')
      .data([inner_radius])
      .enter().append('circle')
        .attr('class', 'arc-inner-circle')
        .attr('cx', 0)
        .attr('cy', 0)
        .attr('r', String);

    // draw the productivity score, if it exists
    if (score) {
      var productivity_score = chart.append('svg:text')
        .attr('class', 'score')
        .text(score)
        .attr('text-anchor', 'middle')
        .attr('y', 16)
        .attr('x', 0);
    }

    jQuery(window).one('resizeDraw', function() {
      productivity_arc(element);
    });
  };


