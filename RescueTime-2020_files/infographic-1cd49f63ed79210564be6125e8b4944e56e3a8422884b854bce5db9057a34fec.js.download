RT.charts = (function(){

  // most charts expect data to be stored in window.RTDATA.chart_data[CHART ID]
  // CHART ID will be specified by the data-chart-id attribute on the element passed in.
  var get_dataset = function(el) {
    var element = jQuery(el);
    var chart_data_id = element.data('chart-id');
    if (typeof(chart_data_id) !== 'undefined' && RTDATA.chart_data !== undefined && RTDATA.chart_data[chart_data_id]) {
      return RTDATA.chart_data[chart_data_id];
    } else {
      console.log('[ERROR] - can not find chart id: ' + chart_data_id);
      return {};
    }
  };

  // get a value from a data attribute and return a value so we don't have to do all this undefined checking.
  var get_data_attribute = function(el, id) {
    var val = jQuery(el).data(id);
    if (typeof(val) === 'undefined' || val === '' || val === 'null') {
      return null;
    } else {
      return val;
    }
  };

  // pass anything into here with either an score attribute, or pass in an index and get a consistent color back
  // if we're looking at the productivity taxonomy, use the prodictivity score colors.
  var chart_item_color = function(d,i,color_range) {
    var activity_colors = d3.scale.category20();

    if (d === 'everything else' || d.display_name === 'everything else') {
      return '#99D4FF';
    } else {
      if (((RTDATA.report_context.taxonomy !== null && RTDATA.report_context.taxonomy.name === 'Productivity') || RTDATA.report_context.taxon !== null) && d.score !== undefined) {
        return RT.productivity_color(d.score);
      } else if (typeof(color_range) !== 'undefined') {
        return color_range(i);
      } else {
        return activity_colors(i);
      }
    }
  };

  // clean up some of this repetitive code. Do some basic chart initialization
  var init_barchart_params = function(element, chart_type) {
    var params = {
      element: $(element).get(0),
      data_id: element.data('chart-id'),
      init_data: get_dataset(element)
    };

    // look for a flag that will tell us not to manipulate the dataset at different screen sizes
    params.never_reduce = get_data_attribute(params.element, 'never-reduce');

    params.data = params.init_data;

    if(Array.isArray(params.data[0]) === false){
      console.log('INVALID DATA FORMAT FOR GROUPED BARCHART!');
    }

    // if there's no data for the chart. bail out.
    if (params.element === undefined || params.data === undefined || (params.data.length === 1 && params.data[0] === undefined)) {
      return false;
    } else {
      // clear out any existing stuff in this element, in case we're replacing
      $(element).html('');

      if (typeof(chart_type) !== undefined) {
        params.chart_type = chart_type;
      } else {
        params.chart_type = 'rank';
      }

      // lots of defaults. override when necessary within the calling function
      params.chart_div = $(params.element).addClass('browsebarchart');
      params.width = params.chart_div.width();
      params.height = params.chart_div.height();
      params.padding = [18,10,100,70];

      if ($(params.chart_div).hasClass('grouped')) {
        params.padding[2] = 42;
      }

      // max duration or percentage of ALL groups
      if (params.chart_type === 'percentage') {
        params.maximum = d3.max(params.data.map(function(d){return d3.max(d, function(dd){return dd.percentage;});})) / 100;
        params.minimum = d3.min(params.data.map(function(d){return d3.min(d, function(dd){return dd.percentage;});})) / 100;
      } else {
        params.maximum = d3.max(params.data.map(function(d){return d3.max(d, function(dd){return dd.duration;});}));
        params.minimum = d3.min(params.data.map(function(d){return d3.min(d, function(dd){return dd.duration;});}));
      }

      params.animation_duration = 750;


      // need to get a start and end time to build the graph correctly.
      params.start_time = RT.time_frame_start();
      params.end_time = RT.time_frame_end();
      params.intervals = RT.time_frame_intervals();
      params.group_padding = 8;
      params.bar_border_radius = 3;
      params.bar_area_height = params.height - (params.padding[0] + params.padding[2]);
      params.bar_area_width = params.width - (params.padding[1] + params.padding[3]);
      params.num_items = d3.max(params.data.map(function(d){return d.length;}));
      params.group_width = (params.bar_area_width / params.num_items) - params.group_padding;

      params.scales = {
        x_interval: d3.time.scale().domain( [params.start_time, params.end_time] ).range( [0, params.bar_area_width] ),
        x_default: d3.scale.ordinal().domain( d3.range( params.num_items )).rangeBands( [0, params.bar_area_width] ),
        y_default: d3.scale.linear().range( [0, params.bar_area_height] ).domain( [0, params.maximum] ),
        //'y_label_duration': d3.scale.linear().domain([0,params['maximum_duration']/60/60]).range([0, params['bar_area_height']]), // like scales.y_duration, except expresses itself in hours instead of seconds. might be a better way of doing this.
        y_stacked_percentage: d3.scale.linear().domain( [0,1] ).range( [0, params.bar_area_height ] ),
        y_collapsed: d3.scale.linear().domain( [0,1] ).range( [0, 0 ] ),
        colors: productivity_color_range,
        comparison_colors: d3.scale.ordinal().range(RT.comparison_colors).domain([0,params.data.length -1])
      };

      // do a couple of conditional things based on the data and chart type
      if (params.chart_type === 'interval') {

        if (params.intervals.length > 20) {
          params.group_padding = 4;
        }
        params.group_width = (params.bar_area_width / params.intervals.length) - params.group_padding;

        //* SET THE X SCALE
        params.scales.x0 = params.scales.x_interval;
      } else {
        // chop the data down if we're on a small display unless never_reduce has been explicitly passed in as a data attribute
        if (params.never_reduce == null) {
          if (params.num_items > 10 || params.width < 670) {
            params.init_data = params.data.map( function(d){ return d.slice(0,10); } );
            params.data = params.init_data;
            params.group_padding = 8;
          } else if (params.num_items < 6) {
            params.group_padding = 48;
          } else {
            params.group_padding = 8;
          }

        // recalculate the x_default scale in case the number of items changed.
        params.num_items = d3.max( params.data.map( function(d){ return d.length; } ));
        params.group_width = (params.bar_area_width / params.num_items) - params.group_padding;
        params.scales.x_default = d3.scale.ordinal().domain( d3.range(params.num_items) ).rangeBands( [0, params.bar_area_width] );

        }

        //* SET THE X SCALE
        params.scales.x0 = params.scales.x_default;
      }

      params.scales.x1 = d3.scale.ordinal().domain( d3.range( params.data.length ) ).rangeBands( [0, params.group_width], 0.05 );
      params.scales.y = params.scales.y_default;
      params.scales.z = d3.scale.category20(); // color picker

      params.bar_width = ((params.group_width - params.group_padding) / params.data.length) + params.data.length;

      params.tooltip = d3.select( params.element ).append("div")
                      .attr('class','barchart-tooltip');
      params.tooltip.append('div')
        .attr('class', 'barchart-tooltip-title');
      params.tooltip.append('div')
        .attr('class', 'barchart-tooltip-details');

      // init the chart svg
      params.svg = d3.select( params.element ).append('svg:svg')
            .attr('width', '100%')
            .attr('height', params.height)
            .attr('viewBox', '0 0 ' + params.width + " " + params.height)
            .attr('preserveAspectRatio', 'none');

      params.chart = params.svg.append('svg:g')
            .attr('transform', 'translate(' + params.padding[3] + ',' + params.padding[0] + ')');

      params.schedule_wrapper = params.chart.append('svg:g').attr('class', 'schedule-wrapper');

      params.label_wrapper = params.chart.append('svg:g').attr('class', 'label-wrapper');
      params.bar_wrapper = params.chart.append('svg:g').attr('class', 'bar-wrapper');

      // not my favorite way to do this, but pull in rolling average chart data if it exists
      // if (RTDATA.chart_data.goal_rolling_average !== undefined) {
      //   params.goal_rolling_average = RTDATA.chart_data.goal_rolling_average;
      // }

      return params;
    }
  };

  var draw_schedule_intervals = function(params) {
    // schedule intervals because why not?
    params.schedule_wrapper.selectAll('.schedule_intervals')
      .data(params.intervals)
      .enter().append('svg:rect')
      .attr('class', function(d) {
        var classes = ['schedule_intervals'];
        var interval_type = 'hour';

        if (RTDATA.report_context.interval !== 'hour') {
          interval_type = 'day';
        }

        if (RT.is_time_within_working_hours(d, interval_type)) {
          classes.push('on-hours');
        } else {
          classes.push('off-hours');
        }
        return classes.join(' ');
      })
      .attr('x', function(d) { return params.scales.x0(d) - (params.group_padding/2); })
      .attr('width', params.group_width + params.group_padding)
      .attr('y', -18)
      .attr('height', params.bar_area_height+18);
  };

  var draw_labels = function(params) {
    var y_ticks = params.scales.y.ticks(4);

    var y_format = function(d) {
      return Math.round((d/60/60)*10)/10+'h';
    };

    if (params.chart_type === 'percentage' || params.scales.y === params.scales.y_stacked_percentage) {
      y_format = function(d) {
        return Math.round(d*100)+'%';
      };
    }

    // y axis labels
    var ylabels = params.label_wrapper.selectAll('.ylabel').data(y_ticks);


    ylabels.enter().append('svg:text');
    ylabels.exit().remove();

    ylabels
      .attr('class', 'ylabel')
      .text(function(d) {
        if (d === 0) {
          return d;
        } else {
          return y_format(d);
        }
      })
      .attr('x', -30)
      .attr('y', function(d) { return (params.bar_area_height - params.scales.y(d)) + 2; })
      .attr('text-anchor', 'end');


    // draw y axis lines
    var ylines = params.label_wrapper.selectAll('.yline').data(params.scales.y.ticks(4));

    ylines.enter().append('line');

    ylines.exit().remove();

    ylines.attr('class', 'yline')
      .attr('x1', -15)
      .attr('y1', function(d) { return params.bar_area_height - params.scales.y(d) - 1; })
      .attr('y2', function(d) { return params.bar_area_height - params.scales.y(d) - 1; })
      .attr('x2', params.bar_area_width)
      .attr('stroke', '#ccc');

    // draw x axis labels
    if (params.chart_type === 'interval') {
      // interval labels
      params.label_wrapper.selectAll('.xlabel')
        .data(label_intervals(params.intervals))
        .enter().append('svg:text')
        .attr('class', 'xlabel')
        .text(function(d) {
          if(RTDATA.report_context.time_frame === 'year' && (RTDATA.report_context.interval === 'week' || RTDATA.report_context.interval === 'day') ) {
            return interval_short_label(d, RTDATA.report_context.interval, RTDATA.report_context.time_frame);
