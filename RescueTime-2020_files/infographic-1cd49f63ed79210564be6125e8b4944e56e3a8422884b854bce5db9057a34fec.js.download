  /* ==== INTERVAL PRODUCTIVITY CHART ==== */
  // ===========================================
  // ===========================================
  // ===========================================
  // ===========================================

  var interval_productivity_chart = function(el) {

    var element = $(el).get(0);
    var data = get_dataset(element);
    // if there's no data for the chart. bail out.
    if (typeof(element) === 'undefined' || typeof(data) === 'undefined') {
      return false;
    }

    // clear out any existing stuff in this element, in case we're replacing
    jQuery(element).html('');

    var chart_div = jQuery(element),
        width = chart_div.width(),
        height = chart_div.height(),
        padding = [6,10,36,20],
        bar_padding = 10,
        colors = RT.productivity_colors.slice(),
        weekday_format = d3.time.format('%a');

    // put a mobile phone color in there as well, right after neutral
    // colors.splice(2, 0, '#9EED8A')

    // need to get a start and end time to build the graph correctly.
    var start_time = RT.time_frame_start(),
        end_time = RT.time_frame_end(),
        intervals = RT.time_frame_intervals();

    // add a tooltip container
    var tooltip = d3.select(element).append("div")
        .attr('class','interval-tooltip');
    tooltip.append('div')
      .attr('class', 'interval-tooltip-name');
    tooltip.append('div')
      .attr('class', 'interval-tooltip-duration');

    // this is kinda gross, but update this flag every time we update the tooltip info so
    // we can avoid flickering.
    var tooltip_text_has_been_updated = false,
        tooltip_total_duration,
        tooltip_detail_duration;

    // some placeholders for the tooltip content that can get updated if needed.
    var tooltip_name = '';
    var tooltip_duration = 0;

    if (intervals.length > 20) {
      bar_padding = 3;
    }

    if (intervals.length > 40) {
      bar_padding = 1;
    }

    var bar_area_height = height-(padding[0]+padding[2]),
        bar_area_width = width-(padding[1]+padding[3]),
        bar_width = (bar_area_width/intervals.length)-bar_padding;

    if (RTDATA.report_context.time_frame === 'year') {
      bar_width = 3;
    }

    // var productivity_levels = d3.layout.stack()(["very_distracting", "distracting", "ios_device", "neutral", "productive", "very_productive"].map(function(level) {
    var productivity_levels = d3.layout.stack()(["very_distracting", "distracting", "neutral", "productive", "very_productive"].map(function(level) {
      return data.map(function(d) {
        var val = {label: level, x: iso(d.interval), total_duration: d.duration, level_duration: (d.productivities[level] || 0) };
        val.y = d.productivities[level] || 0;
        return val;
      });
    }));

    // var yoffset = d3.min(data.map(function(d) { return 0 - d.productivities['distracting'] - d.productivities['very_distracting'];}));
    var max_productive_duration = d3.max(data.map(function(d) {return d3.max([((d.productivities.neutral || 0) + (d.productivities.productive  || 0) + (d.productivities.very_productive || 0)), (d.productivities.ios_device || 0)]);}));
    var max_distracting_duration = d3.max(data.map(function(d) { return (d.productivities.distracting || 0) + (d.productivities.very_distracting || 0); }));
    var max_duration = d3.max(data.map(function(d) { return d.duration; }));
    var ymax = max_productive_duration + max_distracting_duration;

    // prevents baseline from being at top on empty chart
    if(ymax === 0) ymax = 60;

    var x = d3.time.scale().domain([start_time, end_time] ).range([0, bar_area_width]),
        y = d3.scale.linear().range([0, bar_area_height]).domain([0, ymax]);

    var svg = d3.select(element).append("svg:svg")
        .attr("width", '100%')
        .attr("height", height)
        .attr("viewBox", "0 0 "+ width + " " + height)
        .attr("preserveAspectRatio", "none");

    var chart = svg.append("svg:g")
        .attr("transform", "translate(" + padding[3] + "," + (y(ymax)+padding[0]) + ")");

    var schedule_wrapper = chart.append('svg:g').attr('class', 'schedule-wrapper');

    var labels = svg.append("svg:g")
        .attr('class', 'labels')
        .attr("transform", "translate("+ padding[3] + "," + (height + 8) + ")");

    draw_schedule_intervals();

    draw_y_axis_lines();

    // Add a group for each cause.
    var levels = chart.selectAll("g.level")
          .data(productivity_levels)
        .enter().append("svg:g")
          .attr("class", "level")
          .style("fill", function(d, i) { return colors[i]; })
          .on("mousemove", function(d,i) {
            position_interval_tooltip(d,d3.mouse(element));
          })
          .on("mouseout", function() {
            hide_interval_tooltip();
          });

    var height_offset = function(i) {
      var distracting_sum = data[i].productivities.distracting || 0;
      var very_distracting_sum = data[i].productivities.very_distracting || 0;

      // You CAN use this/2 to shift the baseline of the chart to the middle of neutral time
      // not currently implemented
      var neutral_sum = data[i].productivities.neutral || 0;

      return y((distracting_sum+very_distracting_sum)-max_distracting_duration);
    };


    // Add a rect for each date.
    var rect = levels.selectAll("rect")
        .data(Object)
      .enter().append("svg:rect")
        .attr("x", function(d) { return x(d.x); })
        .attr("y", function(d,i) {
          return -y(d.y0) - y(d.y) + height_offset(i);
        })
        .attr("height", function(d) { return y(d.y) + 'px'; })
        .attr("width", bar_width)
        .on("mouseover", function(d) {
          if (d.total_duration !== undefined) {
            var interval_name = interval_full_label(d.x, RTDATA.report_context.interval, RTDATA.report_context.time_frame);
            populate_interval_tooltip(interval_name, d.total_duration, d.label, d.level_duration);
          }
        });


    // BEGIN iOS GREEN LINE
    // set up iOS data with backfilled empty intervals
    var ios_data = [],
        stroke_width = 2,
        circle_radius = 2.5,
        interval_obj;

    for(var data_index = 0; data_index < data.length; data_index++){
      interval_obj = {
        interval: data[data_index].interval,
        duration: (data[data_index].productivities.ios_device === undefined ? 0 : data[data_index].productivities.ios_device),
        total_duration: data[data_index].duration
      };
      ios_data.push(interval_obj);
    }

    if(ios_data.length > 0){
      var ios_color = "#9EED8A",
          ios_wrapper = chart.append('svg:g').attr('class', 'ios-wrapper'),
          ios_line = d3.svg.line()
            .x(function(d) { return x(iso(d.interval)) + (bar_width/2); })
            .y(function(d,i) {
              var line_y;
              if(d.duration > 0){
                line_y = d3.min([(-y(max_distracting_duration) - (stroke_width * 2)), -y(d.duration + max_distracting_duration)]);
              } else {
                line_y = -y(max_distracting_duration);
              }
              return line_y;
            });

      ios_wrapper.append("path")
        .attr("class", "line")
        .attr("d", ios_line(ios_data))
        .attr("fill", "none")
        .attr("stroke", ios_color)
        .attr("stroke-width", stroke_width);

      ios_wrapper.selectAll("circle").data(ios_data).enter().append("circle")
        .attr("cx", function(d) { return x(iso(d.interval)) + (bar_width/2);})
        .attr("cy", function(d) {
          var cir_y;
          if(d.duration > 0){
            cir_y = d3.min([(-y(max_distracting_duration) - (stroke_width * 2)), -y(d.duration + max_distracting_duration)]);
          } else {
            cir_y = -y(max_distracting_duration);
          }
          return cir_y;
        })
        .attr("r", function(d){ return (d.duration > 0 ? circle_radius : 0);})
